function Cgun(Color)
game:GetService("RunService").Stepped:Connect(function()
if bl then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Black()
end
end
if wh then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
White()
end
end
if gld then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Gold()
end
end
if rd then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Red()
end
end
if blu then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Blue()
end
end
if grnn then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Green()
end
end
if pnk then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Pink()
end
end
if volt then
local CheckTool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
if CheckTool then
Violet()
end
end
end)

if Color == "violet" then
volt = true
pnk = false
blu = false
rd = false
gld = false
grnn = false
wh = false
bl = false
end
if Color == "pink" then
volt = false
pnk = true
blu = false
rd = false
gld = false
grnn = false
wh = false
bl = false
end
if Color == "green" then
volt = false
pnk = false
blu = false
rd = false
gld = false
grnn = true
wh = false
bl = false
end
if Color == "blue" then
volt = false
pnk = false
blu = true
rd = false
gld = false
grnn = false
wh = false
bl = false
end
if Color == "red" then
volt = false
pnk = false
blu = false
rd = true
gld = false
grnn = false
wh = false
bl = false
end
if Color == "gold" then
volt = false
pnk = false
blu = false
rd = false
gld = true
grnn = false
wh = false
bl = false
end
if Color == "black" then
volt = false
pnk = false
blu = false
rd = false
gld = false
grnn = false
wh = false
bl = true
end
if Color == "white" then
volt = false
pnk = false
blu = false
rd = false
gld = false
grnn = false
wh = true
bl = false
end
end

function Dong(gun)
	if gun == "ak-47" or gun == "ak47" or gun == "ak" then
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["AK-47"].ITEMPICKUP)
		game.Players.LocalPlayer.Backpack["AK-47"].Parent = game.Players.LocalPlayer.Character
		wait()
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").GripPos = Vector3.new(0.9,2,1)
		wait()
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		wait()
		game.Players.LocalPlayer.Backpack["AK-47"].Parent = game.Players.LocalPlayer.Character
	end
	if gun == "remington-870" or gun == "remington870" or gun == "remington" or gun == "rem" then
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
		game.Players.LocalPlayer.Backpack["Remington 870"].Parent = game.Players.LocalPlayer.Character
		wait()
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").GripPos = Vector3.new(0.9,2,1)
		wait()
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		wait()
		game.Players.LocalPlayer.Backpack["Remington 870"].Parent = game.Players.LocalPlayer.Character
	end
	if gun == "m9" then
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M9"].ITEMPICKUP)
		game.Players.LocalPlayer.Backpack["M9"].Parent = game.Players.LocalPlayer.Character
		wait()
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").GripPos = Vector3.new(0.9,2,0)
		wait()
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		wait()
		game.Players.LocalPlayer.Backpack["M9"].Parent = game.Players.LocalPlayer.Character
	end
	if gun == "long" then
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M9"].ITEMPICKUP)
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["AK-47"].ITEMPICKUP)
		game.Players.LocalPlayer.Backpack["M9"].Parent = game.Players.LocalPlayer.Character
		game.Players.LocalPlayer.Backpack["AK-47"].Parent = game.Players.LocalPlayer.Character
		game.Players.LocalPlayer.Backpack["Remington 870"].Parent = game.Players.LocalPlayer.Character
		wait()
		game.Players.LocalPlayer.Character["M9"].GripPos = Vector3.new(0.9,2,0)
		game.Players.LocalPlayer.Character["AK-47"].GripPos = Vector3.new(0.9,2,3)
		game.Players.LocalPlayer.Character["Remington 870"].GripPos = Vector3.new(0.9,2,6)
		wait()
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = game.Players.LocalPlayer.Backpack
			end
		end
		wait()
		game.Players.LocalPlayer.Backpack["M9"].Parent = game.Players.LocalPlayer.Character
		game.Players.LocalPlayer.Backpack["AK-47"].Parent = game.Players.LocalPlayer.Character
		game.Players.LocalPlayer.Backpack["Remington 870"].Parent = game.Players.LocalPlayer.Character
	end
       if gun == "smoke" then
