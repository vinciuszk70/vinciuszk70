		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").GripPos = Vector3.new(0.9,0.9,-2.3)
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").GripRight = Vector3.new(0,0,0)
		wait()
