function ShowAdmins()
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "viniciuszk70",
Color = Color3.fromRGB(255, 255, 255);
Font = Enum.Font.GothamBlack;
TextSize = 20;
})
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "takkan007",
Color = Color3.fromRGB(255, 255, 255);
Font = Enum.Font.GothamBlack;
TextSize = 20;
})
end

function PrefixMessage()
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "Prefix owner >",
Color = Color3.fromRGB(255, 255, 255);
Font = Enum.Font.GothamBlack;
TextSize = 20;
})
end

function Notify()
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "Hi",
Color = Color3.fromRGB(255, 0, 0);
Font = Enum.Font.GothamBlack;
TextSize = 50;
})
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "Welcome gay",
Color = Color3.fromRGB(255, 0, 0);
Font = Enum.Font.GothamBlack;
TextSize = 50;
})
end

function AbuserNotify()
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "Stop abusing or rejoin",
Color = Color3.fromRGB(255, 0, 0);
Font = Enum.Font.GothamBlack;
TextSize = 20;
})
wait(.3)
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
Text = "Stop abusing or rejoin",
Color = Color3.fromRGB(255, 0, 0);
Font = Enum.Font.GothamBlack;
TextSize = 20;
})
wait(.3)
game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
