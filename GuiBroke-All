Close.Parent = main
Close.BackgroundColor3 = Color3.fromRGB(255, 32, 140)
Close.Position = UDim2.new(0.675000012, 0, 0, 0)
Close.Size = UDim2.new(0, 39, 0, 29)
Close.Text = "X"
Close.TextColor3 = Color3.fromRGB(0, 0, 0)
