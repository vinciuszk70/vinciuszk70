lagserver.Parent = main
lagserver.BackgroundColor3 = Color3.new(0, 0, 0)
lagserver.Position = UDim2.new(0.010818379, 0, 0.118325961, 0)
lagserver.Size = UDim2.new(0, 130, 0, 50)
