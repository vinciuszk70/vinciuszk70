--// script by v3mpx

--// works on free exploits

local prefix = "!"

Looop = nil

local client = game.Players.LocalPlayer
local current_cursor = nil

function load()
    Looop = true
    if Looop then
        game:GetService('RunService'):BindToRenderStep('on', 1, function() 
            client:GetMouse().Icon = "rbxassetid://"..current_cursor
            client:GetMouse().Icon = "http://www.roblox.com/asset?id="..current_cursor
        end)
    end
end

function unload()
    Looop = false
    game:GetService('RunService'):UnbindFromRenderStep('on')
end

function cursorchanger(cursorid)
    if cursorid == nil or '' then unload() end
    current_cursor = cursorid
end


-- cmd stuff

client.Chatted:connect(function(cmd)
    if cmd:match(prefix..'cursor' or prefix..'Cursor') then
        if not prefix then return end
        splitstring = cmd:split(' ')
        cursorcmd = splitstring[1]
        cursorid = splitstring[2]
        if current_cursor == nil then
            cursorchanger(cursorid)
            load()
        elseif current_cursor ~= nil then
            unload()
            cursorchanger(cursorid)
            load()
        end
    end
end)
end)

