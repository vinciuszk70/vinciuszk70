local CPS = Instance.new("ScreenGui")
local CurrentCPS = Instance.new("TextLabel")
local PreviousCPS = Instance.new("TextLabel")

--Properties:

CPS.Name = "CPS"
CPS.Parent = game.Players.LocalPlayer.PlayerGui
CPS.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

CurrentCPS.Name = "CurrentCPS"
CurrentCPS.Parent = CPS
CurrentCPS.AnchorPoint = Vector2.new(0, 1)
CurrentCPS.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
CurrentCPS.Position = UDim2.new(0.01, 0, 0.679999988, 0)
CurrentCPS.Size = UDim2.new(0.089933524, 0, 0.0422509208, 0)
CurrentCPS.Font = Enum.Font.GothamBold
CurrentCPS.Text = "CPS: 0"
CurrentCPS.TextColor3 = Color3.fromRGB(0, 0, 0)
CurrentCPS.TextScaled = true
CurrentCPS.TextSize = 14.000
CurrentCPS.TextWrapped = true

PreviousCPS.Name = "PreviousCPS"
PreviousCPS.Parent = CPS
PreviousCPS.AnchorPoint = Vector2.new(0, 1)
PreviousCPS.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
PreviousCPS.Position = UDim2.new(0.01, 0, 0.729999988, 0)
PreviousCPS.Size = UDim2.new(0.089999998, 0, 0.0500000007, 0)
PreviousCPS.Font = Enum.Font.GothamBold
PreviousCPS.Text = "Previous CPS: 0"
PreviousCPS.TextColor3 = Color3.fromRGB(0, 0, 0)
PreviousCPS.TextScaled = true
PreviousCPS.TextSize = 14.000
PreviousCPS.TextWrapped = true

-- Scripts:

local function LIOXS_fake_script() -- CPS.MainHandler 
	local script = Instance.new('LocalScript', CPS)

	local mouse = game.Players.LocalPlayer:GetMouse()
	local clicks = 0
	
	mouse.Button1Down:Connect(function()
		clicks = clicks + 1
		script.Parent.CurrentCPS.Text = "CPS: "..clicks
	end)
	
	while wait() do
		if clicks > 0 then
			wait(1)
			script.Parent.PreviousCPS.Text = "Previous CPS: "..clicks
			clicks = 0
			script.Parent.CurrentCPS.Text = "CPS: "..clicks
		end
	end
end
coroutine.wrap(LIOXS_fake_script)()
-- CPS.READ ME BEFORE DELETING is disabled.

