if _G.Spam == true then
		if loll and loll.Character then
while _G.Spam == true do wait()
workspace.Remote.arrest:InvokeServer(loll.Character.Torso)
workspace.Remote.arrest:InvokeServer(loll.Character.Torso)
workspace.Remote.arrest:InvokeServer(loll.Character.Torso)
workspace.Remote.arrest:InvokeServer(loll.Character.Torso)
