function Tk(Player)

if not Player.Character:FindFirstChildOfClass("ForceField", true) and Player.Character.Humanoid.Health ~= 0 then

local States = {}

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

States.Tika = true

while wait() do

if States.Tika then

LPlayer.Character.Humanoid.Sit = false

LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame

for i = 1,15 do
game.ReplicatedStorage.meleeEvent:FireServer(Player)
end

if Player.Character.Humanoid.Health == 0 then
States.Tika = false
States = nil
LPlayer.Character.Humanoid.Sit = false
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end

end

end

end

end

function Spy()
enabled = true
spyOnMyself = false
public = false
publicItalics = true
privateProperties = {
Color = Color3.fromRGB(255,255,255); 
Font = Enum.Font.GothamBlack;
TextSize = 16;
}

local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local player = Players.LocalPlayer or Players:GetPropertyChangedSignal("LocalPlayer"):Wait() or Players.LocalPlayer
local saymsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
local getmsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")
local instance = (_G.chatSpyInstance or 0) + 1
_G.chatSpyInstance = instance

local function onChatted(p,msg)
if _G.chatSpyInstance == instance then
if p==player and msg:lower():sub(1,4)=="/spy" then
enabled = not enabled
wait(0.3)
privateProperties.Text = "Msd Spy "..(enabled and "en" or "dis").."abled"
StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
elseif enabled and (spyOnMyself==true or p~=player) then
msg = msg:gsub("[\n\r]",''):gsub("\t",' '):gsub("[ ]+",' ')
local hidden = true
local conn = getmsg.OnClientEvent:Connect(function(packet,channel)
if packet.SpeakerUserId==p.UserId and packet.Message==msg:sub(#msg-#packet.Message+1) and (channel=="All" or (channel=="Team" and public==false and Players[packet.FromSpeaker].Team==player.Team)) then
hidden = false
end
end)
wait(1)
conn:Disconnect()
if hidden and enabled then
if public then
saymsg:FireServer((publicItalics and "/me " or '').."Spy Opion [".. p.Name .."]: "..msg,"All")
else
privateProperties.Text = "Msd Spy [".. p.Name .."]: "..msg
StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
end
end
end
end
end

for _,p in ipairs(Players:GetPlayers()) do
p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end

Players.PlayerAdded:Connect(function(p)
p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end)

privateProperties.Text = "Msd Spy "..(enabled and "en" or "dis").."abled"
StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
if not player.PlayerGui:FindFirstChild("Chat") then wait(3) end
local chatFrame = player.PlayerGui.Chat.Frame
chatFrame.ChatChannelParentFrame.Visible = true
end

function VerifySlotT(Toggle)

if Toggle == "on" then

VerifySlot = true

if Slot1 == true or Slot2 == true or Slot3 == true then

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam

end

end

if Toggle == "off" then

VerifySlot = false

end

end

function ClickKill(Toggle)

if Toggle == "on" then

ClickKillToggle = true

local Mouse = LPlayer:GetMouse()

local function GetPlayerFromCharacter(Character)
for _, Player in pairs(Players:GetPlayers()) do
if Player.Character == Character then
return Player
end
end
end

Mouse.Button1Down:connect(function()
local Obj = Mouse.Target

local Player = GetPlayerFromCharacter(Obj.Parent)

if ClickKillToggle then

if Player ~= nil then

if Player.Character:FindFirstChild("HumanoidRootPart") then

if not Player.Character:FindFirstChildOfClass("ForceField", true) and Player.Character.Humanoid.Health ~= 0 then

local Arg = {}

if Player.Character:FindFirstChild("vest") then
for i = 1,7 do
Arg[i] = {
RayObject = Ray.new(Vector3.new(0,0,0), Vector3.new(0,0,0)),
Distance = 0,
Cframe = CFrame.new(),
Hit = Player.Character.Head
}
end
else
for i = 1,5 do
Arg[i] = {
RayObject = Ray.new(Vector3.new(0,0,0), Vector3.new(0,0,0)),
Distance = 0,
Cframe = CFrame.new(),
Hit = Player.Character.Head
}
end
end

if LPlayer.TeamColor.Name == Player.TeamColor.Name then

local SaveTeam = LPlayer.TeamColor.Name
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Rem = LPlayer.Backpack:FindFirstChild("Remington 870") or LPlayer.Character:FindFirstChild("Remington 870")

game.ReplicatedStorage.ReloadEvent:FireServer(Rem)
for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Rem)

workspace.Remote.loadchar:InvokeServer(nil, SaveTeam)
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam

else

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Rem = LPlayer.Backpack:FindFirstChild("Remington 870") or LPlayer.Character:FindFirstChild("Remington 870")

game.ReplicatedStorage.ReloadEvent:FireServer(Rem)
for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Rem)

end

end

end

end

end

end)

end

if Toggle == "off" then

ClickKillToggle = false

end

end

function Orbit(Gun)

if Gun == "M9" or Gun == "m9" then
Gun = "M9"
end

if Gun == "M4A1" or Gun == "m4" then
Gun = "M4A1"
end

if Gun == "AK-47" or Gun == "ak" then
Gun = "AK-47"
end

if Gun == "AK-47" or Gun == "ak" then
Gun = "AK-47"
end

if Gun == "Remington 870" or Gun == "rem" then
Gun = "Remington 870"
end

ItemHandler:InvokeServer(ItemGiver[Gun].ITEMPICKUP)

while wait() do

local Number = 0.9
local Number2 = 3

if LPlayer.Backpack:FindFirstChild(Gun) then
LPlayer.Backpack:FindFirstChild(Gun).Parent = LPlayer.Character
