wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

if GC == "black" or GC == "green" or GC == "white" or GC == "gold" or GC == "blue" or GC == "red" or GC == "pink" or GC == "violet" then

