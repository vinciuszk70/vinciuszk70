end

if Toggle == "off" then

if Clothing == true then

Clothing = false

if CopPvp == false then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

end

end

end

function Times(Time)

if Time == "day" or Time == "dia" then

if Night == true then
Night = false
end

Day = true

Run.Stepped:connect(function()
if Day then
game.Lighting.FogEnd = 100000
game.Lighting.FogStart = 0
game.Lighting.ClockTime = 14
game.Lighting.Brightness = 2
game.Lighting.GlobalShadows = true
end
end)

end

if Time == "night" or Time == "noite" then

if Day == true then
Day = false
end

Night = true

Run.Stepped:connect(function()
if Night then
game.Lighting.FogEnd = 100000
game.Lighting.FogStart = 0
game.Lighting.ClockTime = 2
game.Lighting.Brightness = 0
end
end)

end

if Time == "off" then

Day = false
Night = false

end

end

function SuperPunch(Toggle)

if Toggle == "on" then

Punch = true

mainRemotes = game.ReplicatedStorage
meleeRemote = mainRemotes['meleeEvent']
mouse = LPlayer:GetMouse()
punching = false
cooldown = false

function punch()
cooldown = true
local part = Instance.new("Part", LPlayer.Character)
part.Transparency = 1
part.Size = Vector3.new(6, 3, 4)
part.CanCollide = false
local w1 = Instance.new("Weld", part)
w1.Part0 = LPlayer.Character.Torso
w1.Part1 = part
w1.C1 = CFrame.new(0,0,2)
part.Touched:connect(function(hit)
if game.Players:FindFirstChild(hit.Parent.Name) then
local plr = game.Players:FindFirstChild(hit.Parent.Name) 
if plr.Name ~= LPlayer.Name then
part:Destroy()

for i = 1,100 do
meleeRemote:FireServer(plr)
end
end
end
end)

wait()
cooldown = false
part:Destroy()
end


mouse.KeyDown:connect(function(key)
if cooldown == false then
if key:lower() == "f" then

if Punch then
punch()
end

end
end
end)

end

if Toggle == "off" then

Punch = false

end

end

function Crash(Arg)

if Arg == "mobiles" or Arg == "mobile" or Arg == "movil" or Arg == "movils" then

CrashPlayers = true

Run.Stepped:Connect(function()

if CrashPlayers then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
local M9 = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

local Locate = {}

for i = 1,10000 do
Locate[i] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

for i = 1,2 do
ShootEvent:FireServer(Locate,M9)
end

end

end)

wait()

CrashPlayers = false

end

if Arg == "players" or Arg == "all" or Arg == "a" or Arg == "everyone" then

CrashPlayers = true

Run.Stepped:Connect(function()

if CrashPlayers then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
local M9 = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

local Locate = {}

for i = 1,100000 do
Locate[i] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

ShootEvent:FireServer(Locate,M9)

end

end)

wait()

CrashPlayers = false

end

if Arg == "sweet" then

CrashSweet = true

Run.Stepped:Connect(function()

for CRS = 1,10 do

if CrashSweet then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local M9 = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

local AGS = {}

for Sweet = 1,100000 do
AGS[Sweet] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

ShootEvent:FireServer(AGS,M9)

end

end

end)

wait(.1)

CrashSweet = false

end

if Arg == "server" or Arg == "servidor" or Arg == "sv" then

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Rem = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for i = 1,1000 do
ShootEvent:FireServer(Arg,Rem)
end

end

end

function LagNumber(Number)
pcall(function()
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
local M9 = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")
local Arrg = {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
["Distance"] = 0, 
["Cframe"] = CFrame.new(), 
["Hit"] = workspace
}
for i = 1,Number do
ShootEvent:FireServer(Arrg,M9)
end
end)
end

function Lag(One,Two)

if One == "server" or One == "sv" or One == "servidor" then

if Two == "on" or Two == "ligado" then

_G.Lag = true

while _G.Lag == true do wait()

LagNumber(2)

end

end

end

if One == "server" or One == "sv" or One == "servidor" then

if Two == "off" or Two == "desligado" then

_G.Lag = false

end

end

if One == "armor" or One == "amr" or One == "swat" then

if Two == "on" or Two == "ligado" then

LagArmor = true

while wait() do

for i = 1,20 do

coroutine.wrap(function()

if LagArmor then

TeamEvent:FireServer("Bright blue")

ItemHandler:InvokeServer(ItemClothes["Riot Police"].ITEMPICKUP)

end

end)()

end

end

end

end

if One == "armor" or One == "amr" or One == "swat" then

if Two == "off" or Two == "desligado" then

LagArmor = false

RespawnSplit("01")

end

end

end

function Invisible(Toggle)

if Toggle == "on" then

local LPlayer = game.Players.LocalPlayer
Char  = LPlayer.Character
touched,tpdback = false, false

LPlayer.CharacterAdded:connect(function(char)
if script.Disabled ~= true then
wait(.25)
loc = Char.HumanoidRootPart.Position
Char:MoveTo(box.Position + Vector3.new(0,.5,0))
end
end)

game:GetService('UserInputService').InputBegan:connect(function(key)
if key.KeyCode == Enum.KeyCode.Equals then
if script.Disabled ~= true then
script.Disabled = true
print'you may re-execute'
end
end
end)

box = Instance.new('Part',workspace)
box.Anchored = true
box.CanCollide = true
box.Size = Vector3.new(10,1,10)
box.Position = Vector3.new(0,700,0)

box.Touched:connect(function(part)
if (part.Parent.Name == LPlayer.Name) then
if touched == false then
touched = true
function apply()
if script.Disabled ~= true then
no = Char.HumanoidRootPart:Clone()
wait(.25)
Char.HumanoidRootPart:Destroy()
no.Parent = Char
Char:MoveTo(loc)
touched = false
end end
if Char then
apply()
end
end
end
end)

repeat wait() until Char
loc = Char.HumanoidRootPart.Position
Char:MoveTo(box.Position + Vector3.new(0,.5,0))

LPlayer.CharacterAdded:Connect(function(newChar)
if AutoInv then
newChar:WaitForChild("HumanoidRootPart",0.3)
wait(2.3)

local LPlayer = game.Players.LocalPlayer
Char  = LPlayer.Character
touched,tpdback = false, false

LPlayer.CharacterAdded:connect(function(char)
if script.Disabled ~= true then
wait(2.3)
loc = Char.HumanoidRootPart.Position
Char:MoveTo(box.Position + Vector3.new(0,.5,0))
end
end)

game:GetService('UserInputService').InputBegan:connect(function(key)
if key.KeyCode == Enum.KeyCode.Equals then
if script.Disabled ~= true then
script.Disabled = true
print'you may re-execute'
end
end
end)

box = Instance.new('Part',workspace)
box.Anchored = true
box.CanCollide = true
box.Size = Vector3.new(10,1,10)
box.Position = Vector3.new(0,700,0)

box.Touched:connect(function(part)
if (part.Parent.Name == LPlayer.Name) then
if touched == false then
touched = true
function apply()
if script.Disabled ~= true then
no = Char.HumanoidRootPart:Clone()
wait(.25)
Char.HumanoidRootPart:Destroy()
no.Parent = Char
Char:MoveTo(loc)
touched = false
end end
if Char then
apply()
end
end
end
end)

repeat wait() until Char
loc = Char.HumanoidRootPart.Position
Char:MoveTo(box.Position + Vector3.new(0,.5,0))
end
end)

AutoInv = true

while wait() do
if AutoInv then
if LPlayer.Character.Humanoid.Sit == true then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(916.297546, 99.9900055, 2383.50928)
end
end
end

end

if Toggle == "off" then

AutoInv = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam

end

end

function CGunWhite()
local M9 = LPlayer.Character:FindFirstChild("M9")
local Rem = LPlayer.Character:FindFirstChild("Remington 870")
local Ak = LPlayer.Character:FindFirstChild("AK-47")
local M4 = LPlayer.Character:FindFirstChild("M4A1")
local Guns = M9 or Rem or Ak or M4

for i = 1,50 do
if Guns then
local Part = Guns:FindFirstChild('Part')
local Model = Guns:FindFirstChild('Model')
if Part then
Part.BrickColor = BrickColor.new("White")
Part.Material = "Neon"
Part.Name = "Part1"
end
if Model then
local PartOfModel = Model:FindFirstChild('Part')
if PartOfModel then
PartOfModel.BrickColor = BrickColor.new("White")
PartOfModel.Material = "Neon"
PartOfModel.Name = "Part1"
end
end
end
end
end

