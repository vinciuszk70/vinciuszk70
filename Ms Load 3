local function GetPlayerString(String)
String = String:lower() -- Turns string to lower case

for Index, Player in pairs(game:GetService("Players"):GetPlayers()) do
if string.sub(String, 1, #String) == string.sub(Player.Name:lower(), 1, #String) then
return(Player)
end
end
end


Close_2.MouseButton1Click:Connect(function()
AntiLag2 = false
SpamToggle.Text = "Spam off"
SpamGui.Visible = false
game:GetService("RunService"):UnbindFromRenderStep("UI Clear")
end)

local function GetPower()
if Power.Text ~= "" or tonumber(Power.Text) then
return(tonumber(Power.Text))
else
return(_G.DefaultPower)
end
end

function CrimSagui(diefaggot,mebitch)
local Player = GetPlayerString(Player.Text)

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if Player ~= nil then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = Player.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = Player.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end

CrimPlayer.MouseButton1Down:connect(function()
CrimSagui(GetPlayerString(Player.Text))
end)

CrimPlayer2.MouseButton1Down:connect(function()
local States = {}
local Player = GetPlayerString(Player.Text)
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

States.Crim2 = true

Run.Stepped:Connect(function()

if States.Crim2 then

if Player then

if LPlayer.Team == game.Teams.Criminals then
RespawnSplit("01")
end

local CSpawn = workspace["Criminals Spawn"].SpawnLocation
CSpawn.CanCollide = false
CSpawn.Transparency = 1
CSpawn.Size = Vector3.new(10,10,10)

CSpawn.CFrame = LPlayer.Character.HumanoidRootPart.CFrame

LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame

game.ReplicatedStorage.meleeEvent:FireServer(Player)

if Player.Team == game.Teams.Criminals then

LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame

States = nil

RespawnSplit("01")

end

end

end

end)

wait(6)

if States.Crim2 == true then

States.Crim2 = false

States = nil

LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame

wait(.1)

RespawnSplit("01")

end

end)

function Goto()
local Player = GetPlayerString(Player.Text)
local Players = game:GetService("Players")
local Plc = Player.Character
local PlL = Players.LocalPlayer
PlL.Character.HumanoidRootPart.CFrame = Plc.HumanoidRootPart.CFrame
end

local function Arrest2(Plr)
coroutine.resume(coroutine.create(function()
pcall(function()
local Player = GetPlayerString(Player.Text)
if Plr and Plr.Name and Plr ~= game.Players.LocalPlayer then
local Playe = Plr

if Playe and Playe.Character and Playe.Character:FindFirstChild("Head") then
local PlrHead = {Playe.Character:FindFirstChild("Head")}

if Playe and PlrHead ~= ({} or nil) then
-- Arrest
local Power =  GetPower()
for i = 1, Power, 1 do
coroutine.resume(coroutine.create(function()
if SpamToggle.Text == "Spam on" then
for i = 1, Power, 1 do
coroutine.resume(coroutine.create(function()
workspace.Remote.arrest:InvokeServer(Player.Character.Head)
end))
end
end
end))
end
end
end
end
end)
end))
end

spawn(function()
game:GetService("RunService"):BindToRenderStep("Ultra Arrest Aura", 1, function()
if SpamToggle.Text == "Spam on" then
local Player = GetPlayerString(Player.Text)
coroutine.resume(coroutine.create(function()
if SpamToggle.Text == "Spam on" then
Arrest2(Player)
end
end))
end
end)
end)

SpamToggle.MouseButton1Click:Connect(function()
if SpamToggle.Text == "Spam off" then
SpamToggle.Text = "Not Found"
Arrest2(GetPlayerString(Player.Text))
SpamToggle.Text = "Spam on"
Goto()
else
SpamToggle.Text = "Spam off"
--game:GetService("RunService"):UnbindFromRenderStep("Ultra Arrest Aura")
end
end)

spawn(function()
game:GetService("RunService"):BindToRenderStep("UI Clear", 1, function() 
pcall(function()
coroutine.resume(coroutine.create(function()
for Index, Player in pairs(game.Players:GetPlayers()) do
coroutine.resume(coroutine.create(function()
if Player.Character and Player.Character:FindFirstChildOfClass("BillboardGui", true) then
for i, v in pairs(Player.Character.Head:GetChildren()) do
coroutine.resume(coroutine.create(function()
if v:IsA("BillboardGui") then
v:Destroy()
end
end))
end
end
end))
end
end))
end)
end)
end)

Run.Stepped:Connect(function()
if AntiLag2 then

for i,v in pairs(Players:GetPlayers()) do

local Name = game.workspace[v.Name]
local NHead = Name:FindFirstChild('Head')

if NHead then
if NHead:FindFirstChild('handcuffedGui') then
for i = 1,1000 do
NHead:FindFirstChild('handcuffedGui'):Remove()
end
end
end

end
end
end)

