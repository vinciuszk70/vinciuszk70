	workspace["Prison_Guard_Outpost"].window:GetChildren()[2],
	workspace["Prison_Guard_Outpost"].window:GetChildren()[3],
	workspace["Prison_Guard_Outpost"].window:GetChildren()[4],
	workspace["Prison_Guard_Outpost"].window:GetChildren()[5],
	workspace["Prison_Guard_Outpost"].window:GetChildren()[6],
	workspace["Prison_Guard_Outpost"].window:GetChildren()[7],
	workspace["Prison_Guard_Outpost"].window:GetChildren()[8],
	workspace["Prison_Cafeteria"].Model.Model:GetChildren()[1],
	workspace["Prison_Cafeteria"].Model.Model:GetChildren()[2],
	workspace["Prison_Cafeteria"].Model.Model:GetChildren()[3],
	workspace["Prison_Cafeteria"].Model.Model:GetChildren()[4],
	workspace["Prison_Cafeteria"].Model.Model:GetChildren()[5],
}

glassParts = {}

for _,v in pairs(glass) do
	table.insert(glassParts,{v,v.Parent})
end

wallParts = {}

for i,v in pairs(game:GetService("Workspace")["Prison_Halls"]:GetChildren()) do 
	if v.Name ~= ("floor") then
		if v.Name ~= ("lights") then
			table.insert(wallParts,{v,v.Parent})
		end
	end
end
for i,v in pairs(game:GetService("Workspace")["Prison_Guard_Outpost"]:GetChildren()) do 
	if v.Name ~= ("floor") then
		if v.Name ~= ("lights") then
			table.insert(wallParts,{v,v.Parent})
		end
	end
end
for i,v in pairs(game:GetService("Workspace")["Prison_Cafeteria"]:GetChildren()) do 
	if v.Name ~= ("floor") then
		if v.Name ~= ("lights") then
			table.insert(wallParts,{v,v.Parent})
		end
	end
end
for i,v in pairs(game:GetService("Workspace")["Prison_Fences"]:GetChildren()) do 
	table.insert(wallParts,{v,v.Parent})
end
for i,v in pairs(game:GetService("Workspace")["Prison_OuterWall"]:GetChildren()) do 
	table.insert(wallParts,{v,v.Parent})
end
for i,v in pairs(game:GetService("Workspace")["Doors"]:GetChildren()) do 
	table.insert(wallParts,{v,v.Parent})
end
for i,v in pairs(game:GetService("Workspace")["Prison_Administration"]:GetChildren()) do 
	if v.Name ~= ("light_floor1") then
		if v.Name ~= ("light_floor2") then
			table.insert(wallParts,{v,v.Parent})
		end
	end
end
for i,v in pairs(game:GetService("Workspace")["Prison_Cellblock"]:GetChildren()) do 
	if v.Name ~= ("c_floor") then
		if v.Name ~= ("a_lights") then
			if v.Name ~= ("b_lights") then
				if v.Name ~= ("c_lights") then
					table.insert(wallParts,{v,v.Parent})
				end
			end
		end
	end
end

function Walls(Toggle)

if Toggle == "on" then
for _,v in pairs(wallParts) do
v[1].Parent = v[2]
end
grass.Parent = game:GetService("Lighting")
end

if Toggle == "off" then
for _,v in pairs(wallParts) do
v[1].Parent = game:GetService("Lighting")
end
grass.Parent = workspace
end

end

function Windows(Toggle)

if Toggle == "on" then
for _,v in pairs(glassParts) do
v[1].Parent = v[2]
end
end

if Toggle == "off" then
for _,v in pairs(glassParts) do
v[1].Parent = game:GetService("Lighting")
end
end

end

function Doors(Toggle)

if Toggle == "on" then
game:GetService("Lighting").Doors.Parent = workspace
end

if Toggle == "off" then
workspace.Doors.Parent = game:GetService("Lighting")
end

end

function GetFirePos(Player)
return {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
["Distance"] = 0,
["Cframe"] = CFrame.new(),
["Hit"] = Player.Character.Head
}
end

function InsertFunction(TP)
local Arg = {}

for i,v in pairs(TP:GetPlayers()) do
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end

end

function ReloadEvent()
local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")
game.ReplicatedStorage.ReloadEvent:FireServer(Gun)
for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end
end

function RespawnSplit(Split)
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Split)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
end

function KillTeam(TP)

if TP == "i" or TP == "inmates" or TP == "presos" then

if LPlayer.TeamColor.Name == "Bright orange" then
RespawnSplit("12")
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for i,v in pairs(game.Teams.Inmates:GetPlayers()) do
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end

ReloadEvent()

ShootEvent:FireServer(Arg,Gun)

end

if TP == "g" or TP == "guards" or TP == "policiais" then

if LPlayer.TeamColor.Name == "Bright blue" then
RespawnSplit("12")
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for i,v in pairs(game.Teams.Guards:GetPlayers()) do
if v.Character:FindFirstChild("vest") then
for i = 1,9 do
table.insert(Arg,GetFirePos(v))
end
else
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end
end

ReloadEvent()

ShootEvent:FireServer(Arg,Gun)

end

if TP == "c" or TP == "criminals" or TP == "criminosos" then

if LPlayer.TeamColor.Name == "Really red" then
RespawnSplit("12")
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for i,v in pairs(game.Teams.Criminals:GetPlayers()) do
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end

ReloadEvent()

ShootEvent:FireServer(Arg,Gun)

end

if TP == "n" or TP == "neutrals" or TP == "neutros" then

if LPlayer.TeamColor.Name == "Medium stone grey" then
RespawnSplit("12")
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for i,v in pairs(game.Teams.Neutral:GetPlayers()) do
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end

ReloadEvent()

ShootEvent:FireServer(Arg,Gun)

end

if TP == "a" or TP == "all" or TP == "everyone" or TP == "todos" then

RespawnSplit("12")

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for i,v in pairs(game.Players:GetPlayers()) do
if v.Character:FindFirstChild("vest") then
for i = 1,9 do
table.insert(Arg,GetFirePos(v))
end
else
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end
end

ReloadEvent()

ShootEvent:FireServer(Arg,Gun)

end

end

function Lk(TP)

if TP == "i" or TP == "inmates" or TP == "presos" then

if LPlayer.TeamColor.Name == "Bright orange" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

_G.i = true

while _G.i == true do wait(.3)

pcall(function()
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

function GetFirePos(Player)
return {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
["Distance"] = 0,
["Cframe"] = CFrame.new(),
["Hit"] = Player.Character.Head
}
end

for i,v in pairs(game.Teams.Inmates:GetPlayers()) do
if v.Character:FindFirstChild("HumanoidRootPart") then
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end
end

local j = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")
game.ReplicatedStorage.ReloadEvent:FireServer(j)

for i, v in next, debug.getregistry() do if type(v) == "table"
then if v.Bullets then v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Gun)
end)
end
end

