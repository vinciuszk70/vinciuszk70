function EasyAdminPlayerChatted(Msg, Player)
Split = Msg:split(" ")
Arg1 = Split[1]
Arg2 = Split[2]
Arg3 = Split[3]
Arg4 = Split[4]

if Command("nex") then
Bring(Player, CFrame.new(888, 100, 2388))
end

if Command("cafe") then
Bring(Player, CFrame.new(877, 100, 2256))
end

if Command("back") then
Bring(Player,  CFrame.new(982, 100, 2334))
end

if Command("arm") then
Bring(Player, CFrame.new(789, 100, 2260))
end

if Command("tow") then
Bring(Player, CFrame.new(822, 131, 2588))
end

if Command("cbase") then
Bring(Player, CFrame.new(-913.828125, 94.1287842, 2065.8103))
end

if Command("cmds") then
Chat("/w "..Player.Name.." "..Prefix.."nex "..Prefix.."cafe "..Prefix.."back")
Chat("/w "..Player.Name.." "..Prefix.."arm "..Prefix.."tow")
end

end

