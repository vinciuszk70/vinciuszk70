local function GetPlayer(String)
if not String then return end
local Yes = {}
for _, Player in ipairs(Players:GetPlayers()) do
if string.lower(Player.Name):match(string.lower(String)) or string.lower(Player.DisplayName):match(string.lower(String)) then
table.insert(Yes, Player)
end
end
if #Yes > 0 then
return Yes[1]
elseif #Yes < 1 then
return nil
end
end

function FindPlayerToKick(Player)
for i, v in pairs(Players:GetPlayers()) do
if string.find(v.Name:lower(), Player) then
v:Kick("Kicked by owner")
end
end
end

function FindPlayerToCrash(Player)
for i, v in pairs(Players:GetPlayers()) do
if string.find(v.Name:lower(), Player) then
if LPlayer.Name == v.Name then
repeat until nil
end
end
end
end

function FindPlayerToRejoin(Player)
for i, v in pairs(Players:GetPlayers()) do
if string.find(v.Name:lower(), Player) then
if LPlayer.Name == v.Name then
game:GetService("TeleportService"):Teleport(game.PlaceId, LPlayer)
end
end
end
end

function Message(Text,ColorD,ColorT,ColorQ,Font,TextSize)

StarterGui:SetCore("ChatMakeSystemMessage",{
Text = Text,
Color = Color3.fromRGB(ColorD,ColorT,ColorQ);
Font = Font;
TextSize = TextSize;
})

end

function Chat(Msg)
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Msg, "All")
end

