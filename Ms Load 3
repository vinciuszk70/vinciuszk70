function CheckPermissions(Player)
Player.Chatted:Connect(function(Msg)
if Admin[Player.UserId] then
AdminPlayerChatted(Msg, Player)
end
if Vip[Player.UserId] then
VipPlayerChatted(Msg, Player)
end
if EasyAdmin[Player.UserId] then
EasyAdminPlayerChatted(Msg, Player)
end
end)
end

for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer then
CheckPermissions(v)
end
end

Players.PlayerAdded:Connect(function(v)
CheckPermissions(v)
end)

LPlayer:GetMouse().KeyDown:connect(function(Key)
if Prefix == "/" then
if Key == ";" then
CmdBox:CaptureFocus()
end
else
if Key == Prefix then
CmdBox:CaptureFocus()
end
end
end)

CmdBox.FocusLost:Connect(function(FocusLost)
if FocusLost then
if CmdBox.Text:sub(1,#Prefix) ~= Prefix then
PlayerChatted(Prefix..CmdBox.Text)
else
PlayerChatted(CmdBox.Text)
end
end
end)

spawn(function()
while wait() do
for i,v in pairs(LoopKill) do
pcall(function()
if v ~= nil then
if v.Player.Character:FindFirstChild("HumanoidRootPart") then
Kill(v.Player)
end
end
end)
end
end
end)

spawn(function()
pcall(function()
while wait() do
for i,v in pairs(LoopTk) do
if v ~= nil then
if v.Player.Character:FindFirstChild("HumanoidRootPart") then
if v.Player.Character.Humanoid.Health ~= 0 then
Tk(v.Player)
end
end
end
end
end
end)
end)

spawn(function()
pcall(function()
while wait(1) do
for i,v in pairs(LoopTs) do
if v ~= nil then
if v.Player.Character:FindFirstChild("HumanoidRootPart") then
if v.Player.Character.Humanoid.Health ~= 0 and v.Player.TeamColor.Name ~= "Bright blue" then
Tase(v.Player)
end
end
end
end
end
end)
end)

