function InfToggle(Toggle)

if Toggle == "on" or Toggle == "ligado" then
Inf = true

Run.Stepped:Connect(function()
if Inf then
local j = LPlayer.Character:FindFirstChildOfClass("Tool")
game.ReplicatedStorage.ReloadEvent:FireServer(j)

for i, v in next, debug.getregistry() do if type(v) == "table"
then if v.Bullets then v.CurrentAmmo = math.huge v.MaxAmmo = math.huge
end
end
end
end
end)
end

if Toggle == "off" or Toggle == "desligado" then
Inf = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

end

function TeamColor(Team)

if Team == "i" then
TeamEvent:FireServer("Bright orange")
end

if Team == "g" then
TeamEvent:FireServer("Bright blue")
end

if Team == "c" then
Bruh = workspace["Criminals Spawn"].SpawnLocation
Bruh.CanCollide = false
Bruh.Transparency = 1
Bruh.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
wait()
Bruh.CanCollide = true
Bruh.Transparency = 0
Bruh.CFrame = CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)
end

if Team == "n" then
TeamEvent:FireServer("Medium stone grey")
end

if Team == "white" then
RespawnSplit("1001")
end

if Team == "black" then
RespawnSplit("1003")
end

if Team == "blue" then
RespawnSplit("307")
end

if Team == "red" then
RespawnSplit("21")
end

if Team == "green" then
RespawnSplit("1020")
end

if Team == "yellow" then
RespawnSplit("1009")
end

if Team == "purple" then
RespawnSplit("1015")
end

if Team == "pink" then
RespawnSplit("1032")
end

if Team == "orange" then
RespawnSplit("1005")
end

if Team == "grey" then
RespawnSplit("2")
end

end

function Arrest(Player)

local PHumanoid = Player.Character:FindFirstChild("HumanoidRootPart")
local PCharacter = Player.Character

if Player.TeamColor.Name == "Really red" then

if PHumanoid then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

repeat
LPlayer.Character.HumanoidRootPart.CFrame = PCharacter.HumanoidRootPart.CFrame
workspace.Remote.arrest:InvokeServer(PCharacter.Head)
until PCharacter.Head:FindFirstChild("handcuffedGui") or PCharacter.Humanoid.Health < 1 or Player.TeamColor.Name ~= "Really red"
LPlayer.Character.Humanoid.Sit = false
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
end

function ArrestA(Everyone)
if Everyone == "a" or Everyone == "all" or Everyone == "everyone" or Everyone == "todos" then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
for i,v in pairs(game.Teams.Criminals:GetPlayers()) do
if v.Name ~= LPlayer.Name then
local i = 10
repeat
wait()
i = i-1
if v.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Sit = false
end
game.Workspace.Remote.arrest:InvokeServer(v.Character.HumanoidRootPart)
LPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1.3)
end
until i == 0
end
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
end

function ALock(Toggle)

if Toggle == "on" then

AutoLock = true

if AutoLock then
if LPlayer.Character:FindFirstChild("ClientInputHandler") then
LPlayer.Character:FindFirstChild("ClientInputHandler"):Destroy()
end
LPlayer.Character.Humanoid.WalkSpeed = 25
LPlayer.Character.Humanoid.JumpPower = 50
end

LPlayer.CharacterAdded:Connect(function(newChar)
if AutoLock then
newChar:WaitForChild("HumanoidRootPart",0.3)
wait(.1)
if LPlayer.Character:FindFirstChild("ClientInputHandler") then
LPlayer.Character:FindFirstChild("ClientInputHandler"):Destroy()
end
LPlayer.Character.Humanoid.WalkSpeed = 25
LPlayer.Character.Humanoid.JumpPower = 50
end
end)
end

if Toggle == "off" then

if AutoLock == true then

AutoLock = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end

function Lock()
if LPlayer.Character:FindFirstChild("ClientInputHandler") then
LPlayer.Character:FindFirstChild("ClientInputHandler"):Destroy()
end
LPlayer.Character.Humanoid.WalkSpeed = 25
LPlayer.Character.Humanoid.JumpPower = 50
end

function UnGodMode()
GodMode = false
end

function GodModeOn()

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local GodPosition = LPlayer.Character.HumanoidRootPart.CFrame
end

GodMode = true

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local CurrentHum = LPlayer.Character:WaitForChild("Humanoid")
CurrentHum.Name = "1"

-- New Humanoid
local NewHum = CurrentHum:Clone()
NewHum.Name = "Humanoid"
NewHum.Parent = CurrentHum.Parent
NewHum.DisplayDistanceType = "None"
NewHum.WalkSpeed = 25

CurrentHum:Destroy()

if LPlayer.Character:FindFirstChild("Animate", true) then
LPlayer.Character:FindFirstChild("Animate", true).Disabled = true
LPlayer.Character:FindFirstChild("Animate", true).Disabled = false
end

if LPlayer.Character:FindFirstChild("ForceField", true) then
LPlayer.Character:FindFirstChild("ForceField", true):Destroy()
end

game.Workspace.Camera.CameraType = "Custom" 
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character:WaitForChild("Humanoid")
end

Run.Stepped:Connect(function()
if GodMode then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
GodPosition = LPlayer.Character.HumanoidRootPart.CFrame
end
end
end)

LPlayer.CharacterAdded:Connect(function()
if GodMode then

GodMode = false

wait(.1)

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
LPlayer.Character.HumanoidRootPart.CFrame = GodPosition
end

GodMode = true

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local CurrentHum = LPlayer.Character:WaitForChild("Humanoid")
CurrentHum.Name = "1"

-- New Humanoid
local NewHum = CurrentHum:Clone()
NewHum.Name = "Humanoid"
NewHum.Parent = CurrentHum.Parent
NewHum.DisplayDistanceType = "None"
NewHum.WalkSpeed = 25

CurrentHum:Destroy()

if LPlayer.Character:FindFirstChild("Animate", true) then
LPlayer.Character:FindFirstChild("Animate", true).Disabled = true
LPlayer.Character:FindFirstChild("Animate", true).Disabled = false
end

if LPlayer.Character:FindFirstChild("ForceField", true) then
LPlayer.Character:FindFirstChild("ForceField", true):Destroy()
end

game.Workspace.Camera.CameraType = "Custom" 
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character:WaitForChild("Humanoid")
end

end
end)

end

function Bring(Player, Position)
if Player == nil or Position == nil then return end

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local C = LPlayer.Character["1"]:Clone()
C.Name = "Humanoid"
C.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local STOP = 0
repeat wait(.1)
STOP = STOP + 1
LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not Player.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not Player.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
local STOP_2 = 0
repeat wait()
STOP_2 = STOP_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = Position
until STOP_2 > 10
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end

function BringA()
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if v.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Sit = false
end

if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local C = LPlayer.Character["1"]:Clone()
C.Name = "Humanoid"
C.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local STOP = 0
repeat wait(.1)
STOP = STOP + 1
LPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not v.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not v.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
local STOP_2 = 0
repeat wait()
STOP_2 = STOP_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
until STOP_2 > 10
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end

function Crim(Player, mebitch)
local Player = GetPlayer(Arg2)

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if Player ~= nil then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = Player.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = Player.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end

function CrimA()
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.TeamColor.Name ~= "Really red" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if v ~= nil then
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = v.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = v.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
end
end

function AntiBring(Toggle)
local BackPack = LPlayer.Backpack:FindFirstChildOfClass("Tool")
local UnValid = Instance.new("Part")
UnValid.Parent = LPlayer.Backpack:FindFirstChildOfClass("Tool")
UnValid.Name = "Valided"

if Toggle == "on" then

ABring = true

LPlayer.Character.ChildAdded:Connect(function(NewTool)
if not NewTool:FindFirstChild("Valided") then
NewTool:Destroy()
end
if not NewTool:FindFirstChild("Hammer") then
NewTool:Destroy()
end
end)

LPlayer.CharacterAdded:Connect(function(newChar)
if ABring then
newChar:WaitForChild("HumanoidRootPart",0.3)
LPlayer.Character.ChildAdded:Connect(function(NewTool)
if not NewTool:FindFirstChild("Valided") then
NewTool:Destroy()
end
if not NewTool:FindFirstChild("Hammer") then
NewTool:Destroy()
end
end)
end
end)
end

if Toggle == "off" then

if ABring == true then

ABring = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end

function InfStam(Toggle)

if Toggle == "on" or Toggle == "ligado" then

Stam = true

if LPlayer then
for i,v in next, getgc() do 
if type(v) == "function" and getfenv(v).script and getfenv(v).script == LPlayer.Character.ClientInputHandler then 
for i2,v2 in next, debug.getupvalues(v) do 
if type(v2) == "number" then 
debug.setupvalue(v, i2, math.huge)
end
end
end
end
end

LPlayer.CharacterAdded:Connect(function(newChar)
if Stam then
newChar:WaitForChild("HumanoidRootPart",0.3)
wait(.1)
if LPlayer then
for i,v in next, getgc() do 
if type(v) == "function" and getfenv(v).script and getfenv(v).script == LPlayer.Character.ClientInputHandler then 
for i2,v2 in next, debug.getupvalues(v) do 
if type(v2) == "number" then 
debug.setupvalue(v, i2, math.huge)
end
end
end
end
end
end
end)
end

if Toggle == "off" or Toggle == "desligado" then

if Stam == true then

Stam = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end

function AutoToggle(Toggle)
if Toggle == "on" then
AutoRespawn = true
end
if Toggle == "off" then
AutoRespawn = false
end
end

function LoopTeam(Team)

if Team == "i" then

if Guard == true or Criminal == true then
Guard = false
Criminal = false
end

Inmate = true

Run.Stepped:Connect(function()
if Inmate then
if LPlayer.TeamColor.Name == "Bright orange" then
else
TeamEvent:FireServer("Bright orange")
end
end
end)

end

if Team == "g" then

if Inmate == true or Criminal == true then
Inmate = false
Criminal = false
end

Guard = true

Run.Stepped:Connect(function()
if Guard then
if LPlayer.TeamColor.Name == "Bright blue" then
else
TeamEvent:FireServer("Bright blue")
end
end
end)

end

if Team == "c" then

if Inmate == true or Guard == true then
Inmate = false
Guard = false
end

Criminal = true

Run.Stepped:Connect(function()
if Criminal then
if LPlayer.TeamColor.Name == "Really red" then
else

Bruh = workspace["Criminals Spawn"].SpawnLocation
Bruh.CanCollide = false
Bruh.Transparency = 1
Bruh.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
wait()
Bruh.CanCollide = true
Bruh.Transparency = 0
Bruh.CFrame = CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)

end
end
end)
end

if Team == "off" then
Criminal = false
Guard = false
Inmate = false
end

end

function ForceField(Toggle)

if Toggle == "on" or Toggle == "ligado" then

_G.FF = true

while _G.FF == true do wait()

TeamEvent:FireServer("Bright blue")

if LPlayer.TeamColor.Name == "Bright blue" then
FFCop = true
else
FFCrim = true
end

if FFCop then

if not LPlayer.Character:FindFirstChildOfClass("ForceField", true) then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer() 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

end

if FFCrim then

if not LPlayer.Character:FindFirstChildOfClass("ForceField", true) then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "1004") 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
if LPlayer.TeamColor.Name == "Really red" then
TeamEvent:FireServer("Medium stone grey")
end
end

end
end
end

if Toggle == "off" or Toggle == "desligado" then

if _G.FF == true then
_G.FF = false

if LPlayer.TeamColor.Name == "Medium stone grey" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12") 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end
end

function AntiCrash(Toggle)

if Toggle == "on" then
LPlayer.PlayerScripts.ClientGunReplicator.Disabled = true
end

if Toggle == "off" then
LPlayer.PlayerScripts.ClientGunReplicator.Disabled = false
end

end

function CopyTeam(Player)

CCP = true

if Player == "off" then
wait(.1)

CCP = false
end

while wait() do

if CCP then

if LPlayer.TeamColor.Name == Player.TeamColor.Name then
else
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Player.TeamColor.Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end
end

function AntiSpam()

Run.Stepped:Connect(function()
if AntiSpam then

if LPlayer.TeamColor.Name == "Really red" then
workspace.Remote.loadchar:InvokeServer(nil, "12") 
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(916.297546, 99.9900055, 2383.50928)
end

if LPlayer.Character.Head:FindFirstChild("handcuffedGui") then
workspace.Remote.loadchar:InvokeServer(nil, "12") 
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(916.297546, 99.9900055, 2383.50928)
end

end
end)

local BackPack = LPlayer.Backpack:FindFirstChildOfClass("Tool")
local UnValid = Instance.new("Part")
UnValid.Parent = LPlayer.Backpack:FindFirstChildOfClass("Tool")
UnValid.Name = "Valided"

LPlayer.Character.ChildAdded:Connect(function(NewTool)
if not NewTool:FindFirstChild("Valided") then
NewTool:Destroy()
end
if not NewTool:FindFirstChild("Hammer") then
NewTool:Destroy()
end
end)

LPlayer.CharacterAdded:Connect(function(newChar)
if ABS then
newChar:WaitForChild("HumanoidRootPart",0.3)
LPlayer.Character.ChildAdded:Connect(function(NewTool)
if not NewTool:FindFirstChild("Valided") then
NewTool:Destroy()
end
if not NewTool:FindFirstChild("Hammer") then
NewTool:Destroy()
end
end)
end
end)

end

function TAntiSpam(Toggle)

if Toggle == "on" then

AntiSpam()

AntiSpam = true

ABS = true

end

if Toggle == "off" then

AntiSpam = false

if ABS == true then
ABS = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer() 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

end

end

Run.Stepped:Connect(function()
if AntiLag then

for i,v in pairs(Players:GetPlayers()) do

local Name = game.workspace[v.Name]
local NHead = Name:FindFirstChild('Head')

if NHead then
if NHead:FindFirstChild('handcuffedGui') then
for i = 1,1000 do
NHead:FindFirstChild('handcuffedGui'):Remove()
end
end
end

if Name:FindFirstChild('vest') then
for i = 1,1000 do
Name:FindFirstChild('vest'):Remove()
end
end

end
end
end)

