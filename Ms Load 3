if Toggle == "on" then

AntiLag = true

end

if Toggle == "off" then

AntiLag = false

end

end

function RemoveScreen(Toggle)

if Toggle == "on" then

Remove = true

Run.Stepped:Connect(function()
if Remove then

local Cam = workspace.CurrentCamera 

LPlayer.PlayerGui.Home.hud.Visible = true
LPlayer.PlayerGui.Home.intro.Visible = false 
LPlayer.PlayerGui.Home.fadeFrame.Visible = false 
Cam.CameraType = Enum.CameraType.Custom
Cam.FieldOfView = 70
Cam.CameraSubject = workspace:WaitForChild(LPlayer.Name,0.4) 
game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.All,true)

end
end)
end

if Toggle == "off" then

Remove = false

end

end

function Tase(Player)
pcall(function()
if Player.Character:FindFirstChild("HumanoidRootPart") and Player.Character.Humanoid ~= 0 then

local SaveTeam = LPlayer.TeamColor.Name
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

workspace.Remote.loadchar:InvokeServer(nil, "Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame

local Taser = LPlayer.Character:FindFirstChild("Taser") or LPlayer.Backpack:FindFirstChild("Taser")


local Arg = {
[1] = {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
["Distance"] = 0, 
["Cframe"] = CFrame.new(), 
["Hit"] = Player.Character.Torso
    }
}

ShootEvent:FireServer(Arg, Taser)


workspace.Remote.loadchar:InvokeServer(nil ,SaveTeam)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end)
end

function TaseAll(Everyone)

if Everyone == "a" or Everyone == "all" or Everyone == "everyone" then
local SaveTeam = LPlayer.TeamColor.Name
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

workspace.Remote.loadchar:InvokeServer(nil, "Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame

local Taser = LPlayer.Character:FindFirstChild("Taser") or LPlayer.Backpack:FindFirstChild("Taser")

function GetPlayer(Player)
return {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
["Distance"] = 0, 
["Cframe"] = CFrame.new(), 
["Hit"] = Player.Character.Head
}
end

local Arg = {}

for i,v in pairs(Players:GetPlayers()) do
for i = 1,1 do
table.insert(Arg,GetPlayer(v))
end
end

ShootEvent:FireServer(Arg,Taser)

workspace.Remote.loadchar:InvokeServer(nil ,SaveTeam)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end

end

function AntiSh()

while wait() do
if AntiShield then
for i,v in pairs(Players:GetPlayers()) do
pcall(function()
if workspace[v.Name].Torso:FindFirstChild("ShieldFolder") then
workspace[v.Name].Torso:FindFirstChild("ShieldFolder"):Destroy()
end
end)
end
end
end

end

function AntShieldT(Toggle)

if Toggle == "on" then

AntiShield = true

AntiSh()

end

if Toggle == "off" then

AntiShield = false

end

end

function AuraInstaKill(Toggle)

if Toggle == "on" then

ak = true

meleeRemote = Replicate['meleeEvent']
mouse = LPlayer:GetMouse()
punching = false
cooldown = false

function punch()
cooldown = true
local Part = Instance.new("Part", LPlayer.Character)
Part.Transparency = 1
Part.Size = Vector3.new(5, 5, 5)
Part.CanCollide = false
local Wl = Instance.new("Weld", Part)
Wl.Part0 = LPlayer.Character.Torso
Wl.Part1 = Part
Wl.C1 = CFrame.new(0,0,0)
Part.Touched:connect(function(Hit)
if Players:FindFirstChild(Hit.Parent.Name) then
local plr = Players:FindFirstChild(Hit.Parent.Name) 
if plr.Name ~= LPlayer.Name then
Part:Destroy()

for i = 1,15 do
meleeRemote:FireServer(plr)
end
end
end
end)

wait()
cooldown = false
Part:Destroy()
end

Run.Stepped:Connect(function()
if ak then
punch()
end
end)

end

if Toggle == "off" then

ak = false

end

end

function UnAura()
KillAura = false
end

function Aura(Toggle)

if Toggle == "on" then

KillAura = true

while wait() do
if KillAura then
for i,v in pairs(Players:GetPlayers()) do
pcall(function()
if v ~= LPlayer then
local Distance = (v.Character:FindFirstChildOfClass("Part").Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 10 then
for i = 1, 15 do
Replicate.meleeEvent:FireServer(v)
end
end
end
end)
end
end
end

end

if Toggle == "off" then
UnAura()
end

end

