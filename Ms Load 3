function Fly(Toggle)

if Toggle == "on" or Toggle == "ligado" then

ReCheckFly = true

bodyGyro = Instance.new("BodyGyro", LPlayer.Character.Torso)
bodyGyro.P = 0
bodyGyro.MaxTorque = Vector3.new(0,0,0)
bodyGyro.CFrame = LPlayer.Character.Torso.CFrame

bodyVelocity = Instance.new("BodyVelocity", LPlayer.Character.Torso)
bodyVelocity.MaxForce = Vector3.new(0,0,0)
bodyVelocity.Velocity = Vector3.new(0,0,0)

Controls = {"w","s","a","d","e","q"}
ControlInd = {1,-1,-1,1,-1,1}
ControlStatus = {w = 0,s = 0,a = 0,d = 0,e = 0,q = 0,}

function StartFly()
pcall(function()
bodyGyro:Destroy()
bodyVelocity:Destroy()
bodyGyro = nil
bodyVelocity = nil
end) pcall(function()
bodyGyro = Instance.new("BodyGyro", LPlayer.Character.Torso)
bodyGyro.P = 0
bodyGyro.MaxTorque = Vector3.new(0,0,0)
bodyGyro.CFrame = LPlayer.Character.Torso.CFrame

bodyVelocity = Instance.new("BodyVelocity", LPlayer.Character.Torso)
bodyVelocity.MaxForce = Vector3.new(0,0,0)
bodyVelocity.Velocity = Vector3.new(0,0,0)

flying = true
bodyGyro.P = 9e4
bodyGyro.MaxTorque = Vector3.new(9e4,9e4,9e4)
bodyGyro.CFrame = LPlayer.Character.Torso.CFrame

bodyVelocity.MaxForce = Vector3.new(9e4,9e4,9e4)
bodyVelocity.Velocity = Vector3.new(0,0,0)

end)
end

function EndFly()
pcall(function()
flying = false

bodyGyro.P = 0
bodyGyro.MaxTorque = Vector3.new(0,0,0)
bodyGyro.CFrame = LPlayer.Character.Torso.CFrame

bodyVelocity.MaxForce = Vector3.new(0,0,0)
bodyVelocity.Velocity = Vector3.new(0,0,0)
LPlayer.Character.Humanoid.PlatformStand = false
bodyGyro:Destroy()
bodyVelocity:Destroy()
bodyGyro = nil
bodyVelocity = nil
end)
end

Run.RenderStepped:Connect(function()
if flying then
bodyGyro.CFrame = CFrame.new(LPlayer.Character.Torso.Position, workspace.CurrentCamera.CFrame.p) * CFrame.Angles(0,math.rad(180),0)
bodyVelocity.Velocity = ((bodyGyro.CFrame.lookVector + Vector3.new(0,0.25,0)) * (ControlStatus['w']+ControlStatus['s'])) + ((bodyGyro.CFrame * CFrame.new(ControlStatus['a']+ControlStatus['d'],(ControlStatus['q']+ControlStatus['e']),0).p) - (bodyGyro.CFrame.p))
LPlayer.Character.Humanoid.PlatformStand = true
end
end)

LPlayer:GetMouse().KeyDown:Connect(function(key)
for i,v in pairs(Controls) do
if key == v then
ControlStatus[key] = ControlInd[i]*FlySpeed*25
end
end
end)

LPlayer:GetMouse().KeyUp:Connect(function(key)
for i,v in pairs(Controls) do
if key == v then
ControlStatus[key] = ControlInd[i]*0
end
end
end)

StartFly()

LPlayer.CharacterAdded:Connect(function()
if ReCheckFly then
wait()
StartFly()
end
end)

end

if Toggle == "off" or Toggle == "desligado" then

ReCheckFly = false

EndFly()

end

end

function NoclipT(Toggle)

if Toggle == "on" then

Noclip = true

Run.Stepped:Connect(function()
if Noclip then
LPlayer.Character:FindFirstChild("Head").CanCollide = false
LPlayer.Character:FindFirstChild("Torso").CanCollide = false
end
end)

end

if Toggle == "off" then

Noclip = false

end

end

function RemoveCam()

local Cam = workspace.CurrentCamera 

LPlayer.PlayerGui.Home.hud.Visible = true
LPlayer.PlayerGui.Home.intro.Visible = false 
LPlayer.PlayerGui.Home.fadeFrame.Visible = false 
Cam.CameraType = Enum.CameraType.Custom
Cam.FieldOfView = 70
Cam.CameraSubject = workspace:WaitForChild(LPlayer.Name,0.4) 
game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.All,true)

end

function RespawnSecurity()
ReCh = true
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
if Clothing == true then
if LPlayer.TeamColor.Name == "Bright blue" then
if LPlayer.Character:FindFirstChild("Clothing") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.Character:FindFirstChild("Humanoid").Health < 1 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if ReCh then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end
else
if LPlayer.TeamColor.Name == "Bright orange" then
if LPlayer.Character:FindFirstChild("Clothing") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright orange")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.Character:FindFirstChild("Humanoid").Health < 1 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright orange")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if ReCh then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright orange")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end
end
end
else
if CopPvp == true then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.TeamColor.Name == "Medium stone grey" then
if LPlayer.PlayerGui.Home.intro.Visible == true then
TeamEvent:FireServer("Bright orange")

workspace.Remote.loadchar:InvokeServer()
RemoveCam()
else
RemoveCam()
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
TeamEvent:FireServer("Bright orange")
workspace.Remote.loadchar:InvokeServer()
wait(.1)
TeamEvent:FireServer("Medium stone grey")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
RemoveCam()
else
if LPlayer.TeamColor.Name == "Bright orange" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
RemoveCam()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end
end
end
end

