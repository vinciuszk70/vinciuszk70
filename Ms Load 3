function Unlk(TP)
if TP == "i" or TP == "inmates" or TP == "presos" then
_G.i = false
end
if TP == "c" or TP == "criminals" or TP == "criminosos" then
_G.c = false
end
if TP == "g" or TP == "guards" or TP == "policiais" then
_G.g = false
end
if TP == "n" or TP == "neutrals" or TP == "neutros" then
_G.n = false
end
if TP == "a" or TP == "all" or TP == "everyone" or TP == "todos" then
_G.a = false
end
end

function Kill(Player)
pcall(function()
if Player.Character:FindFirstChild("HumanoidRootPart") then
if not Player.Character:FindFirstChildOfClass("ForceField", true) and Player.Character.Humanoid.Health ~= 0 then

if Player.TeamColor.Name == LPlayer.TeamColor.Name then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

local AFire = {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
["Distance"] = 0, 
["Cframe"] = CFrame.new(), 
["Hit"] = Player.Character.Head
}

if Player.Character:FindFirstChild("vest") then
for i = 1,9 do
table.insert(Arg,AFire)
end
else
for i = 1,5 do
table.insert(Arg,AFire)
end
end

local j = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")
game.ReplicatedStorage.ReloadEvent:FireServer(j)

for i, v in next, debug.getregistry() do if type(v) == "table"
then if v.Bullets then v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Gun)
end
end
end)
end

function AG(Slot)

if Slot == "slot1" then
Slot1 = true

if Slot2 == true or Slot3 == true then
Slot3 = false
Slot2 = false

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if Slot1 then
if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

wait(3)

if RobloxGui.Backpack.Hotbar["4"].ToolName.Text ~= nil then

if RobloxGui.Backpack.Hotbar["1"].ToolName.Text ~= "Remington 870" then
RespawnCheckGun = true
end

if RobloxGui.Backpack.Hotbar["2"].ToolName.Text ~= "M4A1" then
RespawnCheckGun = true
end

if RobloxGui.Backpack.Hotbar["3"].ToolName.Text ~= "AK-47" then
RespawnCheckGun = true
end

if RobloxGui.Backpack.Hotbar["4"].ToolName.Text ~= "M9" then
RespawnCheckGun = true
end

end
else
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

wait(3)

if RobloxGui.Backpack.Hotbar["3"].ToolName.Text ~= nil then

if RobloxGui.Backpack.Hotbar["1"].ToolName.Text ~= "Remington 870" then
RespawnCheckGun = true
end

if RobloxGui.Backpack.Hotbar["2"].ToolName.Text ~= "AK-47" then
RespawnCheckGun = true
end

if RobloxGui.Backpack.Hotbar["3"].ToolName.Text ~= "M9" then
RespawnCheckGun = true
end

end
end
end

