function UnAntiArrst()
AntiArrest = false
end

function AntiArrst()
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

AntiArrest = true

local function RespawnAntiArrest(CFrame)
local hum = LPlayer.Character:FindFirstChild('Humanoid')
local hr = LPlayer.Character:FindFirstChild('HumanoidRootPart')

if hum and hr then
hr.CFrame = CFrame
end
end

function CheckArrest()
if AntiArrest then
if LPlayer.Character:FindFirstChild('HumanoidRootPart') then
if LPlayer.Character.Head:FindFirstChild('handcuffedGui') then
local SaveTeam = LPlayer.TeamColor.Name
workspace.Remote.loadchar:InvokeServer(nil, SaveTeam)
wait(.1)
RespawnAntiArrest(SaveFrame)
wait(11)
workspace.Remote.loadchar:InvokeServer(nil, SaveTeam)
wait(.1)
RespawnAntiArrest(SaveFrame)
end
end
end
end

Run.Stepped:Connect(function()
if AntiArrest then
if LPlayer.Character:FindFirstChild('HumanoidRootPart') then
if LPlayer.Character.Head:FindFirstChild('handcuffedGui') then
SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
end
end
end
end)

LPlayer.CharacterAdded:Connect(function(newChar)
local Locates = {}
Locates.AntiArrest = false
Locates = nil
local Locates = {}
Locates.AntiArrest = true
while wait() do
if AntiArrest then
if Locates.AntiArrest then
newChar:WaitForChild("HumanoidRootPart",0.3)
CheckArrest()
end
end
end
end)

while wait() do
local Locates = {}
Locates.AntiArrest = false
Locates = nil
local Locates = {}
Locates.AntiArrest = true
if AntiArrest then
if Locates.AntiArrest then
CheckArrest()
end
end
end
end

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

Run.Stepped:Connect(function()
if AutoRespawn == true then
if LPlayer.Character:FindFirstChild('HumanoidRootPart') then
if LPlayer.Character:FindFirstChild("Humanoid").Health < 1 then
else
SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
end
end
end
end)

LPlayer.CharacterAdded:Connect(function()
if AutoRespawn == true then
AutoRespawn = false
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
AutoRespawn = true
end
end)

spawn(function()
while wait() do
if AutoRespawn then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character:FindFirstChild("Humanoid").Health < 1 then
if Clothing == true then
if LPlayer.TeamColor.Name == "Bright blue" then
if LPlayer.Character:FindFirstChild("Clothing") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.Character:FindFirstChild("Humanoid").Health < 1 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if ReCh then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end
else
if LPlayer.TeamColor.Name == "Bright orange" then
if LPlayer.Character:FindFirstChild("Clothing") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright orange")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.Character:FindFirstChild("Humanoid").Health < 1 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright orange")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if ReCh then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright orange")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end
end
end
else
if CopPvp == true then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
TeamEvent:FireServer("Bright blue")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.TeamColor.Name == "Medium stone grey" then
if LPlayer.PlayerGui.Home.intro.Visible == true then
TeamEvent:FireServer("Bright orange")

workspace.Remote.loadchar:InvokeServer()
RemoveCam()
else
RemoveCam()
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
TeamEvent:FireServer("Bright orange")
workspace.Remote.loadchar:InvokeServer()
wait(.1)
TeamEvent:FireServer("Medium stone grey")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
RemoveCam()
else
if LPlayer.TeamColor.Name == "Bright orange" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
RemoveCam()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end
end
end
end
end
end
end)

while wait(2) do
if Slot1 == true or Slot2 == true or Slot3 == true then
if RespawnCheckGun == true then
RespawnCheckGun = false
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
