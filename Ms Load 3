function WalkSpeed(Number)
LPlayer.Character.Humanoid.WalkSpeed = Number
end

function JumpPower(Number)
LPlayer.Character.Humanoid.JumpPower = Number
end

function Gravity(Number)
workspace.Gravity = Number
end

local function Command(Cmd)
return Arg1 == Prefix..Cmd
end

local function PrefixCommand(Cmd)
return Arg1 == "!"..Cmd
end

function PlayerChatted(Msg)
if ScriptToggle then
Split = Msg:split(" ")
Arg1 = Split[1]
Arg2 = Split[2]
Arg3 = Split[3]
Arg4 = Split[4]

if Command("k") or Command("kill") or Command("matar") then

if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "presos" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "policiais" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "criminosos" or Arg2 == "n" or Arg2 == "neutrals" or Arg2 == "neutros" or Arg2 == "a" or Arg2 == "all" or Arg2 == "everyone" or Arg2 == "todos" then

KillTeam(Arg2:lower())

else

Kill(GetPlayer(Arg2))

end

end

if Command("tase") then

if Arg2 == "a" or Arg2 == "all" or Arg2 == "everyone" then

TaseAll(Arg2:lower())

else

Tase(GetPlayer(Arg2))

end

end

if Command("lb") then
local Player = GetPlayer(Arg2)
if Player ~= nil and not LoopBring[Player.UserId] then
LoopBring[Player.UserId] = {Player = Player}
end
end

if Command("unlb") then
local Player = GetPlayer(Arg2)
if Player ~= nil and LoopBring[Player.UserId] then
LoopBring[Player.UserId] = nil
end
end

if Command("sa") then
local Player = GetPlayer(Arg2)
if Player ~= nil and not LoopSa[Player.UserId] then
LoopSa[Player.UserId] = {Player = Player}
end
end

if Command("unsa") then
local Player = GetPlayer(Arg2)
if Player ~= nil and LoopSa[Player.UserId] then
LoopSa[Player.UserId] = nil
end
end

if Command("lp") then
local Player = GetPlayer(Arg2)
if Player ~= nil and not LoopPunish[Player.UserId] then
LoopPunish[Player.UserId] = {Player = Player}
end
end

if Command("unlp") then
local Player = GetPlayer(Arg2)
if Player ~= nil and LoopPunish[Player.UserId] then
LoopPunish[Player.UserId] = nil
end
end

if Command("lts") then
local Player = GetPlayer(Arg2)
if Player ~= nil and not LoopTs[Player.UserId] then
LoopTs[Player.UserId] = {Player = Player}
end
end

if Command("unlts") then
local Player = GetPlayer(Arg2)
if Player ~= nil and LoopTs[Player.UserId] then
LoopTs[Player.UserId] = nil
end
end

if Command("lk") or Command("loopkill") then

if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "presos" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "policiais" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "criminosos" or Arg2 == "n" or Arg2 == "neutrals" or Arg2 == "neutros" or Arg2 == "a" or Arg2 == "all" or Arg2 == "everyone" or Arg2 == "todos" then

Lk(Arg2:lower())

else

local Player = GetPlayer(Arg2)
if Player ~= nil and not LoopKill[Player.UserId] then
LoopKill[Player.UserId] = {Player = Player}
end

end

end

if Command("unlk") or Command("unloopkill") then

if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "presos" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "policiais" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "criminosos" or Arg2 == "n" or Arg2 == "neutrals" or Arg2 == "neutros" or Arg2 == "a" or Arg2 == "all" or Arg2 == "everyone" or Arg2 == "todos" then

Unlk(Arg2:lower())

else

local Player = GetPlayer(Arg2)
if Player ~= nil and LoopKill[Player.UserId] then
LoopKill[Player.UserId] = nil
end

end

end

if Command("rj") or Command("rejoin") or Command("reconectar") then
game:GetService("TeleportService"):Teleport(game.PlaceId, LPlayer)
end

if Command("quit") or Command("leave") or Command("sair") then
game:Shutdown()
end

