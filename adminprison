				"My pipe long 9 inches, But my item long 10 inches, My dad long 12 inches."
			}
			while true do
				wait(trollRate)
				if trollchatEnabled == true then
					local value = math.random(1,#LtrollChat)
					local picked_value = LtrollChat[value]
					chat(""..tostring(picked_value).."")
				end
			end
		end
	elseif arg2 == "off" then
		trollchatEnabled = false
	end
	if command("toilet") then
		teleport(GetPlayerFromString(arg2),CFrame.new(959.131958, 96.6899796, 2444.74927))
		notify(arg1:sub(2,2):upper()..arg1:sub(3).."ed "..GetPlayerFromString(arg2).Name..".")
	end
	if command("snack") then
		teleport(GetPlayerFromString(arg2),CFrame.new(949.114136, 101.051971, 2339.53491))
		notify(arg1:sub(2,2):upper()..arg1:sub(3).."ed "..GetPlayerFromString(arg2).Name..".")
	end
	if command("trap") then
		teleport(GetPlayerFromString(arg2),CFrame.new(-318.588226, 54.2398338, 1967.66064))
		notify(arg1:sub(2,2):upper()..arg1:sub(3).."ed "..GetPlayerFromString(arg2).Name..".")
	end
	if command("teleport") then
		teleport(GetPlayerFromString(arg2),GetPlayerFromString(arg3).Character.HumanoidRootPart.CFrame)
		notify(arg1:sub(2,2):upper()..arg1:sub(3).."ed "..GetPlayerFromString(arg2).Name..".")
	end
	if command("void") then
		teleport(GetPlayerFromString(arg2),CFrame.new(99999999, 999999999, 0))
		notify(arg1:sub(2,2):upper()..arg1:sub(3).."ed "..GetPlayerFromString(arg2).Name..".")
	end
	if command("cut") then
		cut(arg2,arg3)
	end
	if command("givetaser") or command("gts") or command("givetase") then
		giveTaser(GetPlayerFromString(arg2))
	end
	if command("arrest") then
		local player = GetPlayerFromString(arg2)
		local number = tonumber(arg3)

		if funct(arg2,{'all','_a','others'}) then
			for _,v in pairs(game.Players:GetPlayers()) do
				if v ~= game.Players.LocalPlayer then
					arrest(v, number)
				end
			end
		else
			if player then 
				arrest(player, number)
			end
		end
		notify("Arresting "..GetPlayerFromString(arg2).Name)
	end
	if command("spamarrest") or command("sa") then 
		local oof = GetPlayerFromString(arg2) 
		if not oof then oof = getBoolFromString(arg2) end
		if type(oof) == "boolean" and states.spam_arrest then 
			states.spam_arrest = nil 
		elseif typeof(oof) == "Instance" and not states.spam_arrest then 
			local spawn_coords = CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0) + Vector3.new(0, 10, 0)
			local char = workspace:FindFirstChild(UserName) 
			local hr = char and char:FindFirstChild("HumanoidRootPart")
			states.spam_arrest = oof.UserId 
			if oof and char and hr then 
				for i=1,math.huge do 
					wait()
					if not states.spam_arrest or SCRIPT_UNLOADED then
						return 
					end 
					hr.CFrame = spawn_coords
					bring(oof)
					arrest(oof, 30)
				end  
			end
		end 
		notify("Spam Arresting "..GetPlayerFromString(arg2).Name)
	end
	if command("team") or command("t") then
		team(arg2)
	end
	if command("loopkill") or command("lk") then
		local function addPlayer(name)
			player = GetPlayerFromString(name)
			if player and not loopkills[player.UserId] and player ~= game.Players.LocalPlayer then 
				loopkills[player.UserId] = { player = player } 
			end 
		end

		if funct(arg2,{'all','others','_a'}) then
			for _,player in pairs(game.Players:GetPlayers()) do
				addPlayer(player.Name)
			end
			notify("Looping all.")
		elseif funct(arg2,{'guards','police','cops','_g'}) then
			for _,player in pairs(game.Players:GetPlayers()) do
				if player.TeamColor == BrickColor.new('Bright blue') then
					addPlayer(player.Name)
				end
			end
			notify("Looping guards.")
		elseif funct(arg2,{'inmates','prisoners','_i'}) then
			for _,player in pairs(game.Players:GetPlayers()) do
				if player.TeamColor == BrickColor.new('Bright orange') then
					addPlayer(player.Name)
				end
			end
			notify("Looping inmates.")
		elseif funct(arg2,{'criminals','crims','_c'}) then
			for _,player in pairs(game.Players:GetPlayers()) do
				if player.TeamColor == BrickColor.new('Really red') then
					addPlayer(player.Name)
				end
			end
			notify("Looping criminals.")
		elseif funct(arg2,{'neutrals','neuts','skids','_n'}) then
			for _,player in pairs(game.Players:GetPlayers()) do
				if player.TeamColor == BrickColor.new('Medium stone grey') then
					addPlayer(player.Name)
				end
			end
			notify("Looping neutrals.")
		elseif funct(arg2,{'colors','_s'}) then
			for _,player in pairs(game.Players:GetPlayers()) do
				if player.TeamColor.Name ~= ('Really red' or 'Bright orange' or 'Bright blue' or 'Medium stone grey') then
					addPlayer(player.Name)
				end
			end
			notify("Looping colors.")
		else
			if GetPlayerFromString(arg2) then
				addPlayer(GetPlayerFromString(arg2).Name)
				notify("Looping "..GetPlayerFromString(arg2).Name)
			end
		end
	end
	if command("unloopkill") or command("unlk") then
		local player = GetPlayerFromString(arg2)
		if player and loopkills[player.UserId] then 
			loopkills[player.UserId] = nil 
		end 
		notify("Unlooped "..GetPlayerFromString(arg2).Name)
	end
	if command("clearloopkill") or command("clk") then
		loopkills = {}
		notify("Cleared Loop Kills.")
	end
	if command("rf") or command("rapidfire") then
		local sM = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
		sM["MaxAmmo"] = math.huge
		sM["StoredAmmo"] = math.huge
		sM["AmmoPerClip"] = math.huge
		sM["CurrentAmmo"] = math.huge
		sM["Damage"] = math.huge
		sM["FireRate"] = -math.huge
		sM["Range"] = math.huge
		sM["ReloadTime"] = 0
		sM["Bullets"] = 30
		sM["AutoFire"] = true
		notify("RapidFire Enabled.")
	end
	if command("ammoes") or command("ammo") then
		local sM = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
		sM["MaxAmmo"] = tonumber(arg2)
		sM["StoredAmmo"] = tonumber(arg2)
		sM["AmmoPerClip"] = tonumber(arg2)
		sM["CurrentAmmo"] = tonumber(arg2)
		notify("Set all ammoes to ",arg2.."")
	end
	if command("firerate") then
		local sM = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
		sM["FireRate"] = tonumber(arg2)
		notify("Set fire rate to ",arg2.."")
	end
	if command("fling") then
		local s = tonumber(arg2)

		game:GetService('RunService').Stepped:connect(function()
			game.Players.LocalPlayer.Character.Head.CanCollide = false
			game.Players.LocalPlayer.Character.UpperTorso.CanCollide = false
			game.Players.LocalPlayer.Character.LowerTorso.CanCollide = false
			game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = false
		end)
		wait(.1)
		local a = Instance.new("BodyThrust")
		a.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
		a.Force = Vector3.new(s,0,s)
		a.Location = game.Players.LocalPlayer.Character.HumanoidRootPart.Position

		local n = true 
		x = game.Players.LocalPlayer.Character
		while true do
			if n == true then
				for _,v in pairs(x:children()) do
					pcall(function()
						if v.className == "Part" then
							v.CanCollide = false
						end
					end)
				end
			end
			game:service("RunService").Stepped:wait()
		end
		local f = game.Players.LocalPlayer.Character.Humanoid.Health
		local F = 0
		if f == F or f < F then
			n = false
		end
	end
	if command("hit") then
		if funct(arg2,{'all','_a','others'}) then
			HitAll()
		elseif funct(arg2,{'neutrals','_n','neut'}) then
			hitTeam(BrickColor.new('Medium stone grey'))
		elseif funct(arg2,{'inmates','_i','prisoner'}) then
			hitTeam(BrickColor.new('Bright orange'))
		elseif funct(arg2,{'criminal','_c','crim'}) then
			hitTeam(BrickColor.new('Really red'))
		elseif funct(arg2,{'guards','_g','police'}) then
			hitTeam(BrickColor.new('Bright blue'))
		else
			hit(GetPlayerFromString(arg2))
			notify("Hitted "..GetPlayerFromString(arg2).."")
		end
	end
	if command("taser") or command("taze") or command("tase") then
		if funct(arg2,{'all','others','_a'}) then
			taseall()
		elseif funct(arg2,{'inmates','_i','prisoner'}) then
			taseTeam(BrickColor.new('Bright orange'))
		elseif funct(arg2,{'neutrals','_n','neut'}) then
			taseTeam(BrickColor.new('Medium stone grey'))
		elseif funct(arg2,{'criminal','_c','crims'}) then
			taseTeam(BrickColor.new('Really red'))
		else		
			taser(GetPlayerFromString(arg2))
			notify("Tasered "..GetPlayerFromString(arg2).."")
		end
	end
	if command("damages") or command("damage") or command("dams") or command("dam") then
		local sM = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
		sM["Damage"] = tonumber(arg2)
		notify("Set Damages to ",arg2.."")
	end
	if command("bul") or command("bullets") then
		local sM = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
		if states.AUTO_INF_AMMO then
			sM["MaxAmmo"] = math.huge
			sM["StoredAmmo"] = math.huge
			sM["AmmoPerClip"] = math.huge
			sM["CurrentAmmo"] = math.huge
			sM["ReloadTime"] = 0
		end
		sM["Bullets"] = tonumber(arg2)
		notify("Set Bullets to ",arg2..".")
	end
	if command("reload") or command("reloadtimes") or command("reloadtime") or command("rt") then
		local sM = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
		sM["ReloadTime"] = tonumber(arg2)
		notify("Set Reloadtimes to ",arg2.."")
	end
	if command("notify") then
		notify(arg2)
	end
	if command("bring") then
		bring(GetPlayerFromString(arg2))
		notify("Brought "..GetPlayerFromString(arg2).Name)
	end
	if command("clickarrest") or command("clickar") then
		if arg2 == "on" then
			notify("Click arrest on")
			ClickARREST = true
			local mouse = game.Players.LocalPlayer:GetMouse()
			local arrestEvent = game.Workspace.Remote.arrest
			mouse.Button1Down:connect(function()
				if ClickARREST == true then
					local obj = mouse.Target
					local response = arrestEvent:InvokeServer(obj)
				end
			end)
		end
		if arg2 == "off" then
			notify("Click arrest off")
			ClickARREST = false
		end
	end
	if command("flooritems") then
		for i, v in pairs(game.Workspace["Prison_ITEMS"].single:GetChildren()) do
			v:MoveTo(game.Players.LocalPlayer.Character.Torso.Position)
		end
	end
	if command("keycard") or command("key") then
		local A_1 = game:GetService("Workspace")["Prison_ITEMS"].single["Key card"].ITEMPICKUP
		local Event = game:GetService("Workspace").Remote.ItemHandler
		Event:InvokeServer(A_1)
	end
	if command("dot") then
		if funct(arg2,{'all','_a','others'}) then
			dotguards = true
			dotinmates = true
			dotcriminals = true
			dotneutrals = true
		elseif funct(arg2,{'guards','_g','cops'}) then
			dotguards = true
		elseif funct(arg2,{'inmates','_i','prisoners'}) then
			dotinmates = true
		elseif funct(arg2,{'criminals','_c','crims'}) then
			dotcriminals = true
		elseif funct(arg2,{'neutrals','_n','neut'}) then
			dotneutrals = true
		elseif funct(arg2,{'remove'}) then
			removeDot()
			dot = {}
			dotguards = false
			dotinmates = false
			dotcriminals = false
			dotneutrals = false
		else
			local player = GetPlayerFromString(arg2)
			if player and not dot[player.UserId] then
				dot[player.UserId] = { player = player }
				notify("Esp "..player..".")
			end
		end
	end
	if command("mute") then
		chat("/mute "..GetPlayerFromString(arg2).."")
	end
	if command("unmute") then
		chat("/unmute "..GetPlayerFromString(arg2).."")
	end
	if command("punish") then
		local player = GetPlayerFromString(arg2)
		if player and not punish[player.UserId] then
			punish[player.UserId] = { player = player }
			notify("Punished "..player.Name.."")
		end
	end
	if command("giveguns") or command("givegun") or command("getguns") or command("getgun") then
		givegun(GetPlayerFromString(arg2))
	end
	if command("unpunish") then
		local player = GetPlayerFromString(arg2)
		if player and punish[player.UserId] then
			punish[player.UserId] = nil
			notify("UnPunished "..player.Name.."")
		end
	end
	if command("clearpunish") or command("cpn") then
		punish = {}
		notify("Cleared punishes")
	end
	if command("crim") then
		if funct(arg2,{'all','_a','others'}) then
			CrimAll()
		elseif funct(arg2,{'guards','police','_g'}) then
			CrimTeam(BrickColor.new('Bright blue'))
		elseif funct(arg2,{'prisoner','inmates','_i'}) then
			CrimTeam(BrickColor.new('Bright orange'))
		elseif funct(arg2,{'neutrals','neut','_n'}) then
			CrimTeam(BrickColor.new('Medium stone grey'))
		else
			crim(GetPlayerFromString(arg2))
			notify("Crimmed "..GetPlayerFromString(arg2).Name)
		end
	end
end

function AdminPlayerChatted(msg,player)
	split_message = msg:split(" ")
	arg1 = split_message[1]
	arg2 = split_message[2]
	arg3 = split_message[3]
	for i = 4,#split_message do
		arg3 = arg3.." "..split_message[i]
	end
	local old = command
	local command = function(cmd) return old(cmd, player) end 
	if command("prefix") then 
		chat("/w "..player.Name.." Successfully changed your prefix from:   "..admins[player.UserId].settings.prefix.." To:   "..arg2)
		admins[player.UserId].settings.prefix = arg2 
	end
	if command("kill") or command("k") then
		kill(GetPlayerFromString(arg2))
		chat("Notify: Killed "..GetPlayerFromString(arg2).."")
	end
	if command("crimbase") or command("cbase") then
		teleport(player,CFrame.new(-913.828125, 94.1287842, 2065.8103))
	end
	if command("tree") then
		teleport(player,CFrame.new(630, 129.32, 2552.388))
	end
	if command("guns") or command("gun") then
		givegun(player)
	end
	if command("cell") or command("cells") or command("cel") then
		teleport(player,CFrame.new(910,99,2477))
	end
	if command("hit") then
		hit(GetPlayerFromString(arg2))
	end
	if command("taser") or command("taze") or command("tase") then		
		taser(GetPlayerFromString(arg2))
	end
	if command("hallroof") or command("halrof") or command("hallrof") then
		teleport(player,CFrame.new(700, 125, 2389))
	end
	if command("village") or command("houses") then
		teleport(player,CFrame.new(-250, 54, 2484))
	end
	if command("carroof") or command("carrof") or command("carrof") then
		teleport(player,CFrame.new(599.029572, 125.03994, 2508.05737))
	end
	if command("guns") or command("gun") then
		if arg2 == "ak" or arg2 == "ak47" or arg2 == "ak-47" then
			local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP
			local Event = game:GetService("Workspace").Remote.ItemHandler
			Event:InvokeServer(A_1, player)
		end
		if arg2 == "remington" or arg2 == "rem" or arg2 == "remington870" then
			local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP
			local Event = game:GetService("Workspace").Remote.ItemHandler
			Event:InvokeServer(A_1, player)
		end
		if arg2 == "m9" or arg2 == "pistol" then
			local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP
			local Event = game:GetService("Workspace").Remote.ItemHandler
			Event:InvokeServer(A_1, player)
		end
	end
	if command("smite") then
		save_team()
		q2(arg2, math.huge, arg3 or 7)
		wait()
		old_team()
	end
	if command("hell") then
		teleport(GetPlayerFromString(arg2),CFrame.new(-568.503418, 10.8399124, 1414.12463))
		chat("Notify: Teleported "..GetPlayerFromString(arg2).."")
	end
	if command("cafrof") or command("caferoof") or command("cafroof") then
		teleport(player,CFrame.new(944.268, 134.784, 2234))
	end
	if command("cellsroof") or command("celroof") or command("celrof") then
		teleport(player,CFrame.new(933.268, 134.784, 2463.6))
	end
	if command("yardsit") then
		teleport(player,CFrame.new(779,98,2520))
	end
	if command("nexus") or command("nex") then
		teleport(player,CFrame.new(916.297546, 99.9900055, 2383.50928))
	end
	if command("gunroom") then
		teleport(player,CFrame.new(823.666, 102.111, 2275))
	end
	if command("guardroom") or command("groom") then
		teleport(player,CFrame.new(834, 100.333, 2310))
	end
	if command("secretroom") or command("secroom") then
		teleport(player,CFrame.new(-50,20,1300))
	end
	if command("mysroom") or command("mysteryroom") then
		teleport(player,CFrame.new(700, 100.32, 2350))
	end
	if command("lroom") or command("lunchroom") or command("yumroom") then
		teleport(player,CFrame.new(890,103,2225))
	end
	if command("yard") or command("yar") then
		teleport(player,CFrame.new(785.313904, 97.9999466, 2476.40723))
	end
	if command("sewer") or command("sew") then
		teleport(player,CFrame.new(914.87793, 78.7045288, 2411.79102))
	end
	if command("bring") then
		teleport(GetPlayerFromString(arg2),player.Character.HumanoidRootPart.CFrame)
		chat("Notify: Brought "..GetPlayerFromString(arg2).."")
	end
	if command("armory") or command("arm") then
		teleport(player,CFrame.new(782.77533, 99.9900055, 2312.53613))
	end
	if command("gatetower") or command("gtower") then
		teleport(player,CFrame.new(504.029572, 125.03994, 2308.05737))
	end
	if command("gateroom") or command("gtr") then
		teleport(player,CFrame.new(500.592, 100.32, 2250.388))
	end
	if command("gate") then
		teleport(player,CFrame.new(503.799866, 102.03994, 2252.01831))
	end
	if command("back") or command("backnexus") then
		teleport(player,CFrame.new(968.299377, 100.390007, 2325.80688))
	end
	if command("cafe") or command("caf") then
		teleport(player,CFrame.new(924.767944, 99.9899597, 2289.30566))
	end
	if command("underworld") or command("uw") then
		teleport(player,CFrame.new(879, 25.36, 2354))
	end
	if command("roof") or command("rof") then
		teleport(player,CFrame.new(827.423523, 118.990005, 2329.62598))
	end
	if command("tow") or command("tower") then
		if arg2 == nil then
			chat(""..prefix.."tow [count]")
		end
		if arg2 == "" then
			teleport(player,CFrame.new(824.250122, 122.039948, 2587.96094))
		end
		if arg2 == "1" then
			teleport(player,CFrame.new(825, 122, 2592))
		end
		if arg2 == "2" then
			teleport(player,CFrame.new(503, 122, 2589))
		end
		if arg2 == "3" then
			teleport(player,CFrame.new(503, 122, 2070))
		end
		if arg2 == "4" then
			teleport(CFrame.new(823, 122, 2073))
		end
		if arg2 == "5" then
			teleport(player,CFrame.new(1158, 122, 2070))
		end
		if arg2 == "6" then
			teleport(player,CFrame.new(1158, 122, 2590))
		end
	end
	if command("towroof") or command("towerroof") then
		if arg2 == nil then
			chat(""..prefix.."tower [count]")
		end
		if arg2 == "" then
			teleport(player,CFrame.new(824.250122, 144.039948, 2587.96094))
		end
		if arg2 == "1" then
			teleport(player,CFrame.new(825, 144, 2592))
		end
		if arg2 == "2" then
			teleport(player,CFrame.new(503, 144, 2589))
		end
		if arg2 == "3" then
			teleport(player,CFrame.new(503, 144, 2070))
		end
		if arg2 == "4" then
			teleport(CFrame.new(823, 144, 2073))
		end
		if arg2 == "5" then
			teleport(player,CFrame.new(1158, 144, 2070))
		end
		if arg2 == "6" then
			teleport(player,CFrame.new(1158, 144, 2590))
		end
	end
	if command("trash") then
		teleport(GetPlayerFromString(arg2),CFrame.new(365.445374, 10.7605114, 1100.21265))
	end
	if command("framehigh") then
		FrameHigh = arg2
	end
	if command("openguardroom") or command("ogr") then
		states.previous_team = game.Players.LocalPlayer.TeamColor.Name wait(0.3)
		workspace.Remote.TeamEvent:FireServer("Bright blue")
		wait(0.3)
		asd3=game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		wait(0.3)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(827, 100.333, 2347)
		wait(0.3)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=
			CFrame.new(asd3)+Vector3.new(1,0,0)
		wait(0.3)
		if states.previous_team == "Bright blue" or states.previous_team == "Bright orange" or states.previous_team == "Medium stone grey" then 
			workspace.Remote.TeamEvent:FireServer(states.previous_team)
			chat("Notify: Door opened")
		end
	end
	if command("openyard") or command("oyard") then
		save_team()
		savepos()
		wait(.1)
		workspace.Remote.TeamEvent:FireServer("Bright blue")
		wait(.3)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(819.99926757813, 99.990005493164, 2376.9223632813)
		wait(.4)
		wait(.05)
		loadpos()
		old_team()
	end
	if command("toilet") then
		teleport(GetPlayerFromString(arg2),CFrame.new(959.131958, 96.6899796, 2444.74927))
	end
	if command("checkadmin") or command("cadmin") or command("checkad") or command("cad") then
		chat(""..player.Name.." you already have admins!")
	end
	if command("crim") or command("criminal") then
		crim(GetPlayerFromString(arg2))
	end
	if command("arrest") then
		local player = GetPlayerFromString(arg2)
		local number = tonumber(arg3)

		if funct(arg2,{'all','_a','others'}) then
			for _,v in pairs(game.Players:GetPlayers()) do
				if v ~= game.Players.LocalPlayer then
					arrest(v, number)
				end
			end
		end
	end
	if command("loadcframe") or command("loadframe") then
		asd3=game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=
			CFrame.new(asd2)+Vector3.new(1,0,0)
		wait(0,3)
		teleport(GetPlayerFromString(arg2),CFrame.new(asd2))
		wait(0.3)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=
			CFrame.new(asd3)+Vector3.new(1,0,0)
		chat("Notify: Loaded CFrame")
	end
	if command("re") then
		local saveTarget = player
		asd3=game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		local player = saveTarget 
		if player then 
			local char = workspace:FindFirstChild(player.Name)
			local hum = char and char:FindFirstChild("HumanoidRootPart")
			local head = char and char:FindFirstChild("Head")

			local m_char = workspace:FindFirstChild(UserName)
			local m_hum = m_char and m_char:FindFirstChild("HumanoidRootPart")
			if m_char and m_hum and char and (char or head) then 
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = hum and hum.CFrame or head.CFrame
			end
			kill(saveTarget)
			wait(7)
			bring(saveTarget)
			wait(0.5)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=
				CFrame.new(asd3)+Vector3.new(1,0,0)
			chat("Notify: Respawned")
		end
		if command("saveframe") or command("savecframe") then
			asd3=game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			wait(0.3)
			local player = player
			if player then 
				local char = workspace:FindFirstChild(player.Name)
				local hum = char and char:FindFirstChild("HumanoidRootPart")
				local head = char and char:FindFirstChild("Head")

				local m_char = workspace:FindFirstChild(UserName)
				local m_hum = m_char and m_char:FindFirstChild("HumanoidRootPart")
				if m_char and m_hum and char and (char or head) then 
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = hum and hum.CFrame or head.CFrame
				end
			end
			wait(0.3)
			asd2=game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			wait(0.3)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=
				CFrame.new(asd3)+Vector3.new(1,0,0)
			chat("Notify: Saved CFrame")
		end
	end
	if command("arrest") then
		local player = GetPlayerFromString(arg2)
		local number = tonumber(arg3)

		if funct(arg2,{'all','_a','others'}) then
			for _,v in pairs(game.Players:GetPlayers()) do
				if v ~= game.Players.LocalPlayer then
					arrest(v, number)
				end
			end
		else
			if player then 
				arrest(player, number)
			end
		end
	end
	if command("gateroomguard") or command("gtrguard") then
		teleport (GetPlayerFromString(arg2),CFrame.new(500.592, 100.32, 2250.388))
	end
	if command("roomtrap") or command("mysteryroom") then
		teleport (GetPlayerFromString(arg2),CFrame.new(700, 100.32, 2350))
	end
	if command("freeze") then
		teleport (GetPlayerFromString(arg2),CFrame.new(787, 102.111, 2251))
	end
	if command("arrestguard") then
		teleport (GetPlayerFromString(arg2),CFrame.new(-920.9,95,2131.866))
		arrest (GetPlayerFromString(arg2))
	end
	if command("snack") then
		teleport(GetPlayerFromString(arg2),CFrame.new(949.114136, 101.051971, 2339.53491))
	end
	if command("trap") then
		teleport(GetPlayerFromString(arg2),CFrame.new(-318.588226, 54.2398338, 1967.66064))
	end
	if command("void") then
		teleport(GetPlayerFromString(arg2),CFrame.new(99999999, 999999999, 0))
	end
	if command("cmds") then
		chat("/w "..player.Name.." "..player.Name..", The commands you have access to are: "..prefix.."k [player], "..prefix.."loopkill or "..prefix.."lk [player].")
		chat("/w "..player.Name.." "..prefix.."unloopkill or "..prefix.."unlk [player], "..prefix.."to [player] "..prefix.."toilet [player], "..prefix.."snack [player], "..prefix.."trash [player], and "..prefix.."trap [player].") wait(1.0)
		chat("/w "..player.Name.." "..prefix.."[player], "..prefix.."saveframe [player], "..prefix.."loadframe [player]..") wait(0.1)
		chat("/w "..player.Name.." "..prefix.."yard / yar, "..prefix.."cbase / crimbase, "..prefix.."cabseroof / crimbaseroof, "..prefix.."towerroof / towroof [count], "..prefix.."tow / tower [count], "..prefix.."smite [player].")
		chat("/w "..player.Name.." "..prefix.."bring [player], "..prefix.."cafe / caf , "..prefix.."nexus / nex, "..prefix.."cafroof / caferoof / cafrof, "..prefix.."secretroom, "..prefix.."arm / armory, "..prefix.."nexus / nex, "..prefix.."mysteryroom / mysroom.")
		chat("/w "..player.Name.." "..prefix.."sew / sewer, "..prefix.."gunroom / guardroom, "..prefix.."hell [player], "..prefix.."underworld / uw, "..prefix.."punish / unpunish [player], "..prefix.."clearpunish / cpn, "..prefix.."taser [player], "..prefix.."hit [player], "..prefix.."error.")
	end
	if command("error") then
		local value = math.random(1, 463)
		chat("/w "..player.Name.." [*]Errors found : "..tostring(value).."")
	end
	if command("loopkill") or command("lk") then
		local player = GetPlayerFromString(arg2)
		if player and not loopkills[player.UserId] then 
			loopkills[player.UserId] = { player = player } 
		end 
	end
	if command("unloopkill") or command("unlk") then
		local player = GetPlayerFromString(arg2)
		if player and loopkills[player.UserId] then 
			loopkills[player.UserId] = nil 
		end 
	end
	if command("punish") then
		local player = GetPlayerFromString(arg2)
		notify("Punished "..player.."")
		if player and not punish[player.UserId] then
			punish[player.UserId] = { player = player }
		end
	end
	if command("unpunish") then
		local player = GetPlayerFromString(arg2)
		notify("Unpunished "..player.."")
		if player and punish[player.UserId] then
			punish[player.UserId] = nil
		end
	end
	if command("clearpunish") or command("cpn") then
		punish = {}
	end
	if command("clearloopkill") or command("clrlk") then
		loopkills = {}
	end
	if command("to") then
		teleport(player,GetPlayerFromString(arg2).Character.HumanoidRootPart.CFrame)
	end
end

function checkAdmin(plr) 
	local connection = plr.Chatted:Connect(function(str)
		if admins[plr.UserId] then 
			AdminPlayerChatted(str, plr)
		end
	end)
	table.insert(connections, connection)
end

for i,v in pairs(game.Players:GetPlayers()) do 
	if v ~= game.Players.LocalPlayer then 
		checkAdmin(v) 
	end 
end

local connection = game.Players.PlayerAdded:Connect(function(newPlayer)
	checkAdmin(newPlayer)
end)
table.insert(connections, connection) 

connection = game.Players.PlayerRemoving:Connect(function(byePlayer)
	if states.spam_arrest == byePlayer.UserId then 
		states.spam_arrest = nil 
	end 
end)
table.insert(connections, connection)
connection = game.Players.LocalPlayer.Chatted:Connect(LocalPlayerChatted)
table.insert(connections, connection)

connection = game.Players.PlayerAdded:Connect(function(plr)
	plr.Chatted:Connect(function(msg)
		if states.OUTPUT_CHAT then
			game:GetService("TestService"):Message('\t\t['..plr.Name..']: '..msg)
		end
	end)
end)
table.insert(connections, connection)

Input.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		if prefix == "/" then
			if Input.Text:sub(1,1) == ";" then 
				LocalPlayerChatted(Input.Text)
			else
				LocalPlayerChatted(";"..Input.Text)
			end 
			Input.Text = ""
		else
			if Input.Text:sub(1,#prefix) == prefix then 
				LocalPlayerChatted(Input.Text)
			else
				LocalPlayerChatted(prefix..Input.Text)
			end 
			Input.Text = ""
		end
	end
end)

--loops 

SCRIPT_UNLOADED = false

spawn(function()
	while wait() do 
		if SCRIPT_UNLOADED then return end 
		for id,yes in pairs(loopkills) do
			if yes.player.Parent then 
				wait(.5)
				kill(yes.player)
			end
		end
	end
end)

spawn(function()
	if flying then
		endFly()
		wait(.05)
		startFly()
	end
end)

spawn(function()
	while wait() do 
		if SCRIPT_UNLOADED then return end 
		for id,yes in pairs(punish) do
			if yes.player.Parent then 
				SCRIPT_UNLOADED = false
				wait(.5)
				punishplr(yes.player)
			end
		end
	end
end)

spawn(function()
	if states.Auto_Load_Frame then
		loadpos()
	end
end)

spawn(function()
	while wait() do
		if SCRIPT_UNLOADED then return end
		if dotinmates then
			dotTeam(BrickColor.new('Bright orange'))
			wait(.5)
			removeDot()
		end
	end
end)

spawn(function()
	while wait() do
		if SCRIPT_UNLOADED then return end
		if dotneutrals then
			dotTeam(BrickColor.new('Medium stone grey'))
			wait(.5)
			removeDot()
		end
	end
end)

spawn(function()
	while wait() do
		if SCRIPT_UNLOADED then return end
		if dotcriminals then
			dotTeam(BrickColor.new('Really red'))
			wait(.5)
			removeDot()
		end
	end
end)

spawn(function()
	while wait() do
		if SCRIPT_UNLOADED then return end
		if dotguards then
			dotTeam(BrickColor.new('Bright blue'))
			wait(.5)
			removeDot()
		end
	end
end)

spawn(function()
	while wait() do 
		if SCRIPT_UNLOADED then return end 
		for id,yes in pairs(loophits) do
			if yes.player.Parent then 
				wait(.5)
				hit(yes.player)
			end
		end
	end
end)

function humanoidDied()
	if states.AUTO_RESPAWN then 
		noclip = false
		workspace.Remote.loadchar:InvokeServer()
	end
end

local char = workspace:FindFirstChild(UserName)
local rootpart = char:WaitForChild("HumanoidRootPart", 0.3)

connection = game.Players.LocalPlayer.CharacterAdded:Connect(function(newChar)
	if states.AUTO_RESPAWN then 
		noclip = false
		newChar:WaitForChild("Humanoid").Died:Connect(function()
			local lastPos = GetRootOri()
			local lastCam = workspace.CurrentCamera.CFrame
			game:GetService("Workspace").Remote.loadchar:InvokeServer()
			wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastPos
			workspace.CurrentCamera.CFrame = lastCam
			game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
		end)
	end	
end)
table.insert(connections, connection)

connection = game:GetService("Players").LocalPlayer.Idled:connect(function()
	if states.ANTI_AFK then
		game:GetService("VirtualUser"):CaptureController()
		game:GetService("VirtualUser"):ClickButton2(Vector2.new())
	end
end)
table.insert(connections, connection)

char.Humanoid.Died:Connect(humanoidDied)
connection = game.Players.LocalPlayer.CharacterAdded:Connect(function(newChar)
	if states.AUTO_GUNS then
		char = newChar 
		rootpart = newChar:WaitForChild("HumanoidRootPart",0.3)

		if ownsSwat then
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M4A1"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["AK-47"].ITEMPICKUP)

			if game.Players.LocalPlayer.TeamColor == "Bright blue" then
				workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Riot Shield"].ITEMPICKUP)
			end

			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M9"].ITEMPICKUP)
		else
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["AK-47"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M9"].ITEMPICKUP)
		end

		if states.AUTO_INF_AMMO then
			for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				if v:FindFirstChild("GunStates") then
					local sM = require(v.GunStates)
					sM["MaxAmmo"] = math.huge
					sM["StoredAmmo"] = math.huge
					sM["AmmoPerClip"] = math.huge
					sM["CurrentAmmo"] = math.huge
					sM["ReloadTime"] = 0
				end
			end
			for _,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
				if v:FindFirstChild("GunStates") and v:IsA("Tool") then
					local sM = require(v.GunStates)
					sM["MaxAmmo"] = math.huge
					sM["StoredAmmo"] = math.huge
					sM["AmmoPerClip"] = math.huge
					sM["CurrentAmmo"] = math.huge
					sM["ReloadTime"] = 0
				end
			end
		end
	end
	if states.AUTO_LOCK then
		char = newChar 
		rootpart = newChar:WaitForChild("HumanoidRootPart",0.3)
		if game.Players.LocalPlayer.Character:FindFirstChild("ClientInputHandler") then
			game.Players.LocalPlayer.Character:FindFirstChild("ClientInputHandler"):Destroy()
		end
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 25
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	end
	if newChar:WaitForChild("Humanoid",0.3) then 
		newChar.Humanoid.Died:Connect(humanoidDied)
	end
end)
table.insert(connections, connection)

table.insert(connections, game:GetService("RunService").RenderStepped:Connect(function()
	if states.FAST_PUNCH then
		pcall(function()
			getsenv(game.Players.LocalPlayer.Character.ClientInputHandler).cs.isFighting = false
		end)
	end
	if states.ANTI_FLING then
		pcall(function()
			for _,v in pairs(game.Players:GetPlayers()) do
				v.Character["Head"].CanCollide = false
				v.Character["Torso"].CanCollide = false
				v.Character["Left Arm"].CanCollide = false
				v.Character["Right Arm"].CanCollide = false
				v.Character["Left Leg"].CanCollide = false
				v.Character["Right Leg"].CanCollide = false
				v.Character["HumanoidRootPart"].CanCollide = false
			end
		end)
	end	
	if states.noclip then
		pcall(function()
			game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
		end)
	end
	if states.ANTI_SHIELD then
		for _,v in pairs(game.Players:GetPlayers()) do
			pcall(function()
				game:GetService("Workspace")[v.Name].Torso.ShieldFolder:Destroy()
			end)
		end
	end
	if states.auto_guu_invis then
		game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").GripPos = Vector3.new(0,100,0)
	end
	if states.ghost then
		wait(0.5)
		local v = game.Players.LocalPlayer
		v.Character["Head"].Transparency = 0.5
		v.Character["Torso"].Transparency = 0.5
		v.Character["Left Arm"].Transparency = 0.5
		v.Character["Right Arm"].Transparency = 0.5
		v.Character["Left Leg"].Transparency = 0.5
		v.Character["Right Leg"].Transparency = 0.5
		v.Character["HumanoidRootPart"].Transparency = 0.5
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Accessory") then
				v.Transparency = 0.5
			end
		end
	end
	if states.invis then
		local v = game.Players.LocalPlayer
		v.Character["Head"].Transparency = 1
		v.Character["Torso"].Transparency = 1
		v.Character["Left Arm"].Transparency = 1
		v.Character["Right Arm"].Transparency = 1
		v.Character["Left Leg"].Transparency = 1
		v.Character["Right Leg"].Transparency = 1
		v.Character["HumanoidRootPart"].Transparency = 1
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if (v:IsA("Accessory")) then
				v:Destroy()
			end
		end
		if states.anti_fell then
			if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.Y > 2500 then
				game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-913.828125, 94.1287842, 2065.8103)
			end
		else
			if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.Y < 0 then
				game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-913.828125, 94.1287842, 2065.8103)
			end
		end
		if View ~= nil then
			pcall(function()
				workspace.CurrentCamera.CameraSubject = View.Character.Head
			end)
		end
		if states.ANTI_INVIS then
			for _,v in pairs(game.Players:GetPlayers()) do
				if v ~= game.Players.LocalPlayer then
					pcall(function()
						if v.Character.HumanoidRootPart.Position.Y > 5000 then
							game.Players.StarterGui:SetCore("SendNotification", {
								Title = "Suspected Invis Player.",
								Text = v.Name,"is a suspected skid.",
								Icon = game.Players:GetUserThumbnailAsync(v.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420),
							})
							kill(v)
						end
					end)
				end
			end
		end
	end
end))

coroutine.wrap(function()
	while wait(0.5) do
		if states.KILLAURA then
			for _,v in pairs(game.Players:GetPlayers()) do
				pcall(function()
					if workspace:FindFirstChild(game.Players.LocalPlayer.Name) and ((game.Players.LocalPlayer.Character:FindFirstChildOfClass('Part').Position-workspace[v.Name]:FindFirstChildOfClass('Part').Position).magnitude < 10) and v ~= game.Players.LocalPlayer then
						for i = 1,workspace[v.Name].Humanoid.Health/5 do
							game:GetService("ReplicatedStorage").meleeEvent:FireServer(v)
						end
					end
				end)
			end
		end
	end
end)()
coroutine.wrap(function()
	while wait(0.5) do
		if states.ArrestAura then
			for _,v in pairs(game.Players:GetPlayers()) do
				pcall(function()
					if workspace:FindFirstChild(game.Players.LocalPlayer.Name) and ((game.Players.LocalPlayer.Character:FindFirstChildOfClass('Part').Position-workspace[v.Name]:FindFirstChildOfClass('Part').Position).magnitude < 10) and v ~= game.Players.LocalPlayer then
						if v.TeamColor.Name == "Bright blue" or v.TeamColor.Name == "Really red" or v.Name ~= game.Players.LocalPlayer then
							game.Workspace.Remote.arrest:InvokeServer(v)
						end
					end
				end)
			end
		end
	end
end)()
coroutine.wrap(function()
	while wait(0.15) do
		if states.KILLAURA_GUARDS then
			for _,v in pairs(game.Players:GetPlayers()) do
				pcall(function()
					if v.TeamColor.Name == "Bright blue" then
						if workspace:FindFirstChild(game.Players.LocalPlayer.Name) and ((game.Players.LocalPlayer.Character:FindFirstChildOfClass('Part').Position-workspace[v.Name]:FindFirstChildOfClass('Part').Position).magnitude < 10) and v ~= game.Players.LocalPlayer then
							for i = 1,workspace[v.Name].Humanoid.Health/5 do
								game:GetService("ReplicatedStorage").meleeEvent:FireServer(v)
							end
						end
					end
				end)
			end
		end
	end
end)()
coroutine.wrap(function()
	while wait(0.15) do
		if states.KILLAURA_INMATES then
			for _,v in pairs(game.Players:GetPlayers()) do
				pcall(function()
					if v.TeamColor.Name == "Bright orange" then
						if workspace:FindFirstChild(game.Players.LocalPlayer.Name) and ((game.Players.LocalPlayer.Character:FindFirstChildOfClass('Part').Position-workspace[v.Name]:FindFirstChildOfClass('Part').Position).magnitude < 10) and v ~= game.Players.LocalPlayer then
							for i = 1,workspace[v.Name].Humanoid.Health/5 do
								game:GetService("ReplicatedStorage").meleeEvent:FireServer(v)
							end
						end
					end
				end)
			end
		end
	end
end)()
coroutine.wrap(function()
	while wait(0.15) do
		if states.KILLAURA_CRIMES then
			for _,v in pairs(game.Players:GetPlayers()) do
				pcall(function()
					if v.TeamColor.Name == "Really red" then
						if workspace:FindFirstChild(game.Players.LocalPlayer.Name) and ((game.Players.LocalPlayer.Character:FindFirstChildOfClass('Part').Position-workspace[v.Name]:FindFirstChildOfClass('Part').Position).magnitude < 10) and v ~= game.Players.LocalPlayer then
							for i = 1,workspace[v.Name].Humanoid.Health/5 do
								game:GetService("ReplicatedStorage").meleeEvent:FireServer(v)
							end
						end
					end
				end)
			end
		end
	end
end)()
coroutine.wrap(function()
	while wait(0.15) do
		if states.KILLAURA_NEUTRALS then
			for _,v in pairs(game.Players:GetPlayers()) do
				pcall(function()
					if v.TeamColor.Name == "Medium stone grey" then
						if workspace:FindFirstChild(game.Players.LocalPlayer.Name) and ((game.Players.LocalPlayer.Character:FindFirstChildOfClass('Part').Position-workspace[v.Name]:FindFirstChildOfClass('Part').Position).magnitude < 10) and v ~= game.Players.LocalPlayer then
							for i = 1,workspace[v.Name].Humanoid.Health/5 do
								game:GetService("ReplicatedStorage").meleeEvent:FireServer(v)
							end
						end
					end
				end)
			end
		end
	end
end)()

local function CallBack(Text, Player)
	if Text == "Refresh" then
		savepos()
		loadchar()
		wait()
		loadpos()
	end
end

local Invoke = Instance.new("BindableFunction")
Invoke.OnInvoke = CallBack

game.Players.PlayerAdded:Connect(function(Player)
	if PlayerJoinedNotify then
		game.StarterGui:SetCore("SendNotification", {
			Title = "Game Notification",
			Text = Player.Name.." / "..Player.DisplayName.." (Id : "..Player.UserId..") Joined The Server",
			Icon = game.Players:GetUserThumbnailAsync(Player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420),
			Button1 = "Ok",
		})
	end
end)

game.Players.PlayerRemoving:Connect(function(Player)
	if PlayerJoinedNotify then
		game.StarterGui:SetCore("SendNotification", {
			Title = "Game Notification",
			Text = Player.Name.." / "..Player.DisplayName.." (Id : "..Player.UserId..") Left The Server",
			Icon = game.Players:GetUserThumbnailAsync(Player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420),
			Button1 = "Ok",
		})
	end
end)

for i,v in pairs(game.Players:GetPlayers()) do
	v.CharacterAdded:Connect(function(Char)
		Char:WaitForChild("Humanoid").Died:Connect(function(Died)
			if PlayerJoinedNotify then
				if v.Name == game.Players.LocalPlayer.Name then
					game.StarterGui:SetCore("SendNotification", {
						Title = "Game Notification",
						Text = "You Has Died",
						Icon = game.Players:GetUserThumbnailAsync(v.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size352x352),
						Button1 = "Refresh",
						Button2 = "Ok",
						Callback = Invoke,
					})
				elseif v.Name ~= game.Players.LocalPlayer.Name then
					game.StarterGui:SetCore("SendNotification", {
						Title = "Game Notification",
						Text = v.Name.." / "..v.DisplayName.." (Id : "..v.UserId..") Died",
						Icon = game.Players:GetUserThumbnailAsync(v.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size352x352),
						Button1 = "Ok",
					})
				end
			end
		end)
	end)
end

getgenv().UNLOAD_ADMIN = function() 
	EnableShootEvent()
	ScorpionAdmin:Destroy()
	Watermark:Destroy()
	states = {}
	for index, connection in pairs(connections) do 
		if typeof(connection) == "RBXScriptConnection" then 
			connection:disconnect()
		elseif typeof(connection) == "string" then 
			pcall(function()
				game:service("RunService"):UnbindFromRenderStep(connection)
			end)
		end
	end
	SCRIPT_UNLOADED = true 
	warn("Successfully unloaded the admin!") 
end 
end)

