openmain.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
openmain.Position = UDim2.new(0.627791524, 0, 0.932539701, 0)
openmain.Size = UDim2.new(0, 100, 0, 27)

TextButton.Parent = openmain
TextButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextButton.Size = UDim2.new(0, 100, 0, 27)
TextButton.Font = Enum.Font.Jura
