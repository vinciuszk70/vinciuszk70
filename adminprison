		game:GetService("Players").LocalPlayer:GetMouse().KeyDown:connect(function(key)
			if key == "q" then
				if Main.Visible == true then
					Main.Visible = false
				else
					if Main.Visible == false then
						Main.Visible = true
					end
				end
			end
		end)
