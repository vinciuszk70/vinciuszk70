			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1158, 144, 2590)
			notify("Teleported to tower roof.")
		end
	end
	if command("crimbaseroof") or command("cbaseroof") or command("cbaserof") then
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-944,125,2068)
		notify("Teleported to criminalbase roof.")
