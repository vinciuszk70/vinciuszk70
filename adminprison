	if command("loopogr") or command("loopopenguardroom") then
		notify("Looping open guard room")
		asd3=game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		states.previous_team = game.Players.LocalPlayer.TeamColor.Name
		_G.loop_ogrd = true
		while true do
			wait(1)
			if _G.loop_ogrd == true then
				wait(0.3)
				workspace.Remote.TeamEvent:FireServer("Bright blue")
				wait(0.3)
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(827, 100.333, 2347)
			end
		end
	end
	if command("unloopogr") or command("unloopopenguardroom") then
		_G.loop_ogrd = false
		notify("Stopped loop open guard room")
		if states.previous_team == "Bright orange" or states.previous_team == "Medium stone grey" then 
			workspace.Remote.TeamEvent:FireServer(states.previous_team)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=
				CFrame.new(asd3)+Vector3.new(1,0,0)
		end
	end
