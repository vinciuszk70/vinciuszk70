-- Scripts:

local function FZFU_fake_script() -- White.LocalScript 
	local script = Instance.new('LocalScript', White)

	White.MouseButton1Down:connect(function()
		saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		local tbl_main = 
			{
				game:GetService("Players").LocalPlayer, 
				"White"
			}
		game:GetService("Workspace").Remote.loadchar:InvokeServer(unpack(tbl_main))
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	end)
end
coroutine.wrap(FZFU_fake_script)()
local function IRBYT_fake_script() -- Black.LocalScript 
	local script = Instance.new('LocalScript', Black)

	Black.MouseButton1Down:connect(function()
		saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		local tbl_main = 
			{
				game:GetService("Players").LocalPlayer, 
				"Black"
			}
		game:GetService("Workspace").Remote.loadchar:InvokeServer(unpack(tbl_main))
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	end)
end
coroutine.wrap(IRBYT_fake_script)()
local function EULW_fake_script() -- Guns.LocalScript
local script = Instance.new('LocalScript', Guns)

    Guns.MouseButton1Down:connect(function()
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M4A1"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["AK-47"].ITEMPICKUP)
			workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["M9"].ITEMPICKUP)
end)
end
coroutine.wrap(EULW_fake_script)()
local function ZUAMN_fake_script() -- Respawn.LocalScript 
	local script = Instance.new('LocalScript', Respawn)

	Respawn.MouseButton1Down:connect(function()
        local lastPos = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").position
        local plr = game.Players.LocalPlayer.Name
        local gayevent = game:GetService("Workspace").Remote.loadchar
        gayevent:InvokeServer(plr)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(lastPos)
	end)
end
coroutine.wrap(ZUAMN_fake_script)()
