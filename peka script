Name = "Peka" -- Nick in text Label
PekaColor = Color3.fromRGB(0, 170, 0) -- Text Color
Color01 = Color3.fromRGB(0, 0, 0) -- Background Color
Color02 = Color3.fromRGB(0, 170, 0) -- Borde Collor
